    game:GetService("RunService").Heartbeat:Connect(function()
        if RespawnActive == true then
            pcall(function()
                game.Players.LocalPlayer.PlayerGui.ScreenGui.Enabled = true
                local savedres = "."..game:GetService("CoreGui").Horizon.Settings.ActionSlideFrame.HealthRSliderFrame.HealthRSlider.HealthRLabel.Text
                if game.Players.LocalPlayer.Character.Humanoid.Health <= game.Players.LocalPlayer.Character.Humanoid.MaxHealth * savedres then
                    RespawnAP()
                end
            end)
        end
    end)
end)

