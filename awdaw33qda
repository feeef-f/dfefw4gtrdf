Horizon.Script9.Size = UDim2.new(0.525, 0, 0.225, 0)
Horizon.Script9.Visible = false
Horizon.Script9.MouseButton1Click:Connect(function()
    TrainJS = not TrainJS
    if TrainJS then
        if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
            Horizon.Script9Title.TextColor3 = Color3.fromRGB(10, 10, 10)
            Horizon.Script9.BackgroundColor3 = Color3.fromRGB(69, 255, 113)
        else
            Horizon.Script9Title.TextColor3 = Color3.fromRGB(233, 233, 233)
            Horizon.Script9.BackgroundColor3 = Color3.fromRGB(144, 41, 246)
        end
        while TrainJS do
            if not game.CoreGui:FindFirstChild("Horizon") then break end
			task.wait(0.5)
			local args = {[1] = {[1] = "Weight",[2] = "Weight"..math.random(4,4)}}
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
			for _,v in pairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
			    if v.Name == "LeftWeight4" or v.Name == "RightWeight4" then
			        v.Transparency = 1
			    end
			end
		end
    else
        Horizon.Script9.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        Horizon.Script9Title.TextColor3 = Color3.fromRGB(233, 233, 233)
        TrainJS = false
    end
end)

