            infoEl3.Background.InfoLabel.Text = "Psychic: +"..api.suffixstring(game.Players.LocalPlayer.PrivateStats.PsychicPower.Value - psychicstat)
        end
    end)

	Horizon.PpBg.MouseEnter:Connect(function()
		infoEl3.Visible = true
		infoEl3.Position = UDim2.new(0, button.AbsolutePosition.X + (button.AbsoluteSize.X / 2), 0, button.AbsolutePosition.Y)
	end)
	Horizon.PpBg.MouseLeave:Connect(function()
		infoEl3.Visible = false
	end)
end

