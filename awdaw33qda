local SavedQualityLevel = settings().Rendering.QualityLevel
local PreTrain = false
local TrainPPDouble = false
local TrainFSDouble = false

spawn(function()
    while task.wait(1) do
        pcall(function()
            if PreTrain == false then
                SavedQualityLevel = settings().Rendering.QualityLevel
            end
        end)
    end
end)

spawn(function()
    if LastPosition ~= false then
        local hesw = math.huge
        game.ReplicatedStorage.RemoteEvent:FireServer({"Respawn"})
        task.wait(1)
        game.Lighting.Blur.Enabled = false
        game.Players.LocalPlayer.PlayerGui.IntroGui.Enabled = false
        game.Players.LocalPlayer.PlayerGui.ScreenGui.Enabled = true
        local char = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart", hesw)
        task.wait(1.3)
        spawn(function()
            hrp.CFrame = CFrame.new(table.unpack(LastPosition.Pos:split(",")))
            LastPosition = false
            UpdateSaves()
            spawn(function()
                if SuperAura then
                    args = {[1] = {[1] = "ConcealRevealAura"}}
                    game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
                elseif NoAura then
                    task.wait(1.25)
                    args = {[1] = {[1] = "ConcealRevealAura"}}
                    game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
                end
            end)
            task.wait(1)
            if TrainFSBR == true then
                TrainFS = true
                TrainFSBR = false
                if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
                    Horizon.Script7.BackgroundColor3 = Color3.fromRGB(69, 255, 113)
                    Horizon.Script7Title.TextColor3 = Color3.fromRGB(10, 10, 10)
                else
                    Horizon.Script7.BackgroundColor3 = Color3.fromRGB(144, 41, 246)
                    Horizon.Script7Title.TextColor3 = Color3.fromRGB(233, 233, 233)
                end
                PrepareTrain()
                task.wait(0.1)
                TrainFSS()
            elseif TrainPPBR == true then
                TrainFSBR = false
                if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
                    Horizon.Script8.BackgroundColor3 = Color3.fromRGB(69, 255, 113)
                    Horizon.Script8Title.TextColor3 = Color3.fromRGB(10, 10, 10)
                else
                    Horizon.Script8Title.TextColor3 = Color3.fromRGB(255, 255, 255)
                    Horizon.Script8.BackgroundColor3 = Color3.fromRGB(144, 41, 246)
                end
                PrepareTrain()
                task.wait(0.1)
                TrainPP = true
                TrainPPP()
            end
        end)
    end
end)

repeat task.wait() until game.Players.LocalPlayer.Character
game.Players.LocalPlayer.OsPlatform = "Horizon"

Horizon.LoadingText.Text = "Horizon is getting ready."

function NewNotification(text, time, color)
    spawn(function()
        local NotificationFrameParent = Instance.new("Frame")
        local NotificationFrame = Instance.new("Frame")
        local NotificationFrameCorner = Instance.new("UICorner")
        local NotificationText = Instance.new("TextLabel")
        local NotificationUIStroke = Instance.new("UIStroke")
        local NotificationUIGradient = Instance.new("UIGradient")
        local SecretButton = Instance.new("TextButton")
    
        NotificationFrameParent.Name = "NotificationFrameParent"
        NotificationFrameParent.Parent = Horizon.Notifications
        NotificationFrameParent.AnchorPoint = Vector2.new(0.5, 0.5)
        NotificationFrameParent.BackgroundTransparency = 1
        NotificationFrameParent.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
        NotificationFrameParent.Position = UDim2.new(0, 0, 0.1, 0)
        NotificationFrameParent.Size = UDim2.new(0.9, 0, 0.08, 0)
        NotificationFrameParent.Visible = true
    
        NotificationFrame.Name = "NotificationFrame"
        NotificationFrame.Parent = NotificationFrameParent
        NotificationFrame.AnchorPoint = Vector2.new(0.5, 0.5)
        NotificationFrame.BackgroundTransparency = 0.1
        NotificationFrame.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
        NotificationFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
        NotificationFrame.Size = UDim2.new(1, 0, 0.9, 0)
        NotificationFrame.Visible = true
    
        NotificationFrameCorner.CornerRadius = UDim.new(0, 10)
        NotificationFrameCorner.Name = "NotificationFrameCorner"
        NotificationFrameCorner.Parent = NotificationFrame
    
        NotificationText.Name = "NotificationText"
        NotificationText.Parent = NotificationFrame
        NotificationText.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        NotificationText.BackgroundTransparency = 1
        NotificationText.TextTransparency = 0
        NotificationText.Position = UDim2.new(0.05, 0, 0.2, 0)
        NotificationText.Size = UDim2.new(0.9, 0, 0.6, 0)
        NotificationText.Font = Enum.Font.GothamMedium
        NotificationText.Text = text
        NotificationText.TextColor3 = Color3.fromRGB(233, 233, 233)
        NotificationText.TextScaled = true
        NotificationText.TextSize = 14.000
        NotificationText.TextWrapped = true

        SecretButton.Name = ":3"
        SecretButton.Parent = NotificationText
        SecretButton.BackgroundTransparency = 1
        SecretButton.TextTransparency = 1
        SecretButton.Size = UDim2.new(1, 0, 1, 0)
        SecretButton.MouseButton1Click:Connect(function()
            NotificationText.Text = "You found a secret 💀"
        end)

        NotificationUIStroke.Parent = NotificationFrame
        NotificationUIStroke.Thickness = 2.5
        if color == "rainbow" then
            NotificationUIStroke.Parent = NotificationFrame
            NotificationUIStroke.Thickness = 2.5
            NotificationUIStroke.Color = Color3.fromRGB(255, 255, 255)
            NotificationUIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 0, 0)), ColorSequenceKeypoint.new(0.16, Color3.fromRGB(242, 110, 2)), ColorSequenceKeypoint.new(0.32, Color3.fromRGB(246, 255, 0)), ColorSequenceKeypoint.new(0.48, Color3.fromRGB(43, 255, 0)), ColorSequenceKeypoint.new(0.64, Color3.fromRGB(0, 162, 255)), ColorSequenceKeypoint.new(0.8, Color3.fromRGB(162, 0, 255)), ColorSequenceKeypoint.new(1, Color3.fromRGB(246, 255, 0))}
            NotificationUIGradient.Parent = NotificationUIStroke
        elseif color == "" then
            NotificationUIStroke.Color = Color3.fromRGB(0, 0, 0)
        elseif color == "purple" then
            NotificationUIStroke.Color = Color3.fromRGB(144, 89, 244)
        elseif color == "green" then
            NotificationUIStroke.Color = Color3.fromRGB(100, 200, 100)
        elseif color == "red" then
            NotificationUIStroke.Color = Color3.fromRGB(200, 100, 100)
        end   
        game:GetService("TweenService"):Create(NotificationFrame, TweenInfo.new(1, Enum.EasingStyle.Elastic), {Position = UDim2.new(0.5, 0, -1.3, 0)}):Play()
        task.wait(time)
        game:GetService("TweenService"):Create(NotificationFrame, TweenInfo.new(1, Enum.EasingStyle.Elastic), {Position = UDim2.new(-0.7, 0, -1.3, 0)}):Play()
        delay(1, function()
            NotificationFrameParent:Destroy()
        end)
    end)
end

function round(num, numDecimalPlaces)
	local mult = 10^(numDecimalPlaces or 0)
	return math.floor(num * mult + 0.5) / mult
end

function converttoletter(num)
	if num / 1e21 >=1 then
		newnum = num / 1e21
		return round(newnum, 6).. "Sx"
	elseif num / 1e18 >=1 then
		newnum = num / 1e18
		return round(newnum, 6).. "Qi"
	elseif num / 1e15 >=1 then
		newnum = num / 1e15
		return round(newnum, 6).. "Qa"
	elseif num / 1e12 >=1 then
		newnum = num / 1e12
		return round(newnum, 6).. "T"
	elseif num / 1e09 >=1 then
		newnum = num / 1e09
		return round(newnum, 6).. "B"
	elseif num / 1e06 >=1 then
		newnum = num / 1e06
		return round(newnum, 6).. "M"
	elseif num / 1e03 >=1 then
		newnum = num / 1e03
		return round(newnum, 6).. "K"
	else return num
	end
end

local api = {
    ['suffixstring'] = function(st)
        local suffixes = {"K", "M", "B", "T", "Qa"}
        for i = #suffixes, 1, -1 do
            local mp = math.pow(10, i * 3)
            if st >= mp then
                return ("%.1f"):format(st / mp) .. suffixes[i]
            end
        end
        return tostring(st)
    end
}

local fists = game:GetService("Players").LocalPlayer.PrivateStats.FistStrength.Value
local psyp = game:GetService("Players").LocalPlayer.PrivateStats.PsychicPower.Value

local PPTrainValue
local FSTrainValue

spawn(function()
    if psyp >= 1000000 and psyp <= 1000000000 then
        PPTrainValue = 3
    elseif psyp >= 1000000000 and psyp <= 1000000000000 then
        PPTrainValue = 4
    elseif psyp >= 1000000000000 and psyp <= 1000000000000000 then
        PPTrainValue = 5
    else
        PPTrainValue = 6
    end

    if fists >= 0 and fists <= 1000 then
        FSTrainValue = 2
    elseif fists >= 5000 and fists <= 1000000000 then
        FSTrainValue = 3
    elseif fists >= 1000000000 and fists <= 100000000000 then
        FSTrainValue = 4
    elseif fists >= 100000000000 and fists <= 10000000000000 then
        FSTrainValue = 5
    else
        FSTrainValue = 6
    end

    while task.wait(10) do
        if not game.CoreGui:FindFirstChild("Horizon") then break end
        psyp = game:GetService("Players").LocalPlayer.PrivateStats.PsychicPower.Value
        if psyp >= 1000000 and psyp <= 1000000000 then
            PPTrainValue = 3
        elseif psyp >= 1000000000 and psyp <= 1000000000000 then
            PPTrainValue = 4
        elseif psyp >= 1000000000000 and psyp <= 1000000000000000 then
            PPTrainValue = 5
        elseif psyp >= 1000000000000000 then
            PPTrainValue = 6
        end
    end

    while task.wait(10) do
        if not game.CoreGui:FindFirstChild("Horizon") then break end
        fists = game:GetService("Players").LocalPlayer.PrivateStats.FistStrength.Value
        if fists >= 0 and fists <= 1000 then
            FSTrainValue = 2
        elseif fists >= 5000 and fists <= 1000000000 then
            FSTrainValue = 3
        elseif fists >= 1000000000 and fists <= 100000000000 then
            FSTrainValue = 4
        elseif fists >= 100000000000 and fists <= 10000000000000 then
            FSTrainValue = 5
        else
            FSTrainValue = 6
        end
    end
end)

function PrepareTrain()
    PreTrain = true
    setfpscap(45)
    settings().Rendering.QualityLevel = "Level01"
    if BeforeTrainPos == false then
       bftposition = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    elseif bftposition == nil then
        bftposition = CFrame.new(table.unpack(BeforeTrainPos.Pos:split(",")))
        BeforeTrainPos = false
    end
end

function TrainPPP()
    if psyp >= 0 and psyp <= 1000000 then
        print(":P")
    elseif psyp >= 1000000 and psyp <= 1000000000 then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2529, 5487, -533)
    elseif psyp >= 1000000000 and psyp <= 1000000000000 then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2560, 5501, -439)
    elseif psyp >= 1000000000000 and psyp <= 1000000000000000 then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2582, 5517, -504)
    elseif psyp >= 1000000000000000 then 
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2553.78125, 5412.83691, -497.583679, -0.392698675, -6.60916344e-09, -0.919667184, 9.85756614e-08, 1, -4.92783663e-08, 0.919667184, -1.10008351e-07, -0.392698675)
    end
end

function TrainFSS()
    if fists >= 0 and fists <= 1000 then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(409, 271, 978)
    elseif fists >= 5000 and fists <= 1000000000 then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2278, 1943, 1054)
    elseif fists >= 1000000000 and fists <= 100000000000 then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1176, 4789, -2293)
    elseif fists >= 100000000000 and fists <= 10000000000000 then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1381, 9274, 1647)
    elseif fists >= 10000000000000 then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-357.586121, 15733.5156, -19.664072, 0.573040724, 0, 0.819526911, 0, 1, 0, -0.819526911, 0, 0.573040724)
    end
end

local StopCamera = false
local Godmoded = false
local Respawning = false
local Spawned = true

spawn(function()
    while task.wait() do
        if not game.CoreGui:FindFirstChild("Horizon") then break end
        if Godmoded == false then
            if Respawning == true then
                game.Workspace.Camera.CameraType = "Scriptable"
            else
                game.Workspace.Camera.CameraType = "Custom"
            end
        end
    end
end)

function RespawnAP()
    pcall(function()
        if Spawned == false then return end
        Spawned = false
        Respawning = true
        if getgenv().GrossFurryStack ~= true and game.Players.LocalPlayer.Character and not TrainFS and not TrainPP then
            oldposition = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        end
        game.ReplicatedStorage.RemoteEvent:FireServer({"Respawn"})
        task.wait()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1194.60498, 74686.8906, 3100.53247, -0.997010887, 0.00568253966, -0.0770515501, 1.44715587e-10, 0.997291505, 0.0735500902, 0.0772608072, 0.0733302459, -0.994310558)
        task.wait(1.45)
    end)
end

spawn(function()
    if getgenv().GrossFurryStack == true then return end
    game.Players.LocalPlayer.CharacterAdded:Connect(function()
        repeat task.wait() until game.Players.LocalPlayer.Character.HumanoidRootPart
        Spawned = true
        task.wait(0.35)
        if oldposition ~= nil then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldposition
        end
        Respawning = false
        spawn(function()
            if SuperAura then
                args = {[1] = {[1] = "ConcealRevealAura"}}
                game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
            elseif NoAura then
                task.wait(1.75)
                args = {[1] = {[1] = "ConcealRevealAura"}}
                game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
            end
        end)
        task.wait(1.5)
        if TrainFS == true then
            PrepareTrain()
            task.wait(0.15)
            TrainFSS()
        elseif TrainPP == true then
            PrepareTrain()
            task.wait(0.15)
            TrainPPP()
        end
    end)
end)

local uis = game:GetService("UserInputService")
local result
local stop = false

function makeSlider(slider, min, max, axis, default, resetButton, label, text, event)
    if stop == true then repeat task.wait() until stop == false end
	local filling = slider:FindFirstChild("Filling")
	local Button = Instance.new("TextButton")
	local dragging = false
	Button.Name = "Button"
	Button.Parent = slider
	Button.AnchorPoint = Vector2.new(0.5, 0.5)
	Button.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Button.BackgroundTransparency = 1.000
	Button.Position = UDim2.new(0.5, 0, 0.5, 0)
	Button.Size = UDim2.new(1, 0, 1, 0)
	Button.ZIndex = 10
	Button.Font = Enum.Font.GothamMedium
	Button.Text = ""
	Button.TextColor3 = Color3.fromRGB(0, 0, 0)
	Button.TextSize = 14.000

	Button.MouseButton1Down:Connect(function()
		dragging = true
	end)
	uis.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			if dragging then dragging = false end
		end
	end)
	uis.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			if dragging then
				local mouseLoc = uis:GetMouseLocation()
				local resultNum
				if axis == "X" then
					local relativePos = mouseLoc - slider.AbsolutePosition
					local percentage = math.clamp(relativePos.X / slider.AbsoluteSize.X, 0.1, 1)
					filling.Visible = false
					if percentage ~= 0 then filling.Visible = true end
					filling.Size = UDim2.new(percentage, 0, 1, 0)
					resultNum = percentage * max
				else
					local relativePos = mouseLoc - (slider.AbsolutePosition + slider.AbsoluteSize / 3.5)
					local percentage = math.clamp(relativePos.Y / slider.AbsoluteSize.Y, 0, 1)
					filling.Visible = false
					if percentage ~= 1 then filling.Visible = true end
					filling.Size = UDim2.new(1, 0, 1 - percentage, 0)
					resultNum = 1 - percentage * max
				end
				if resultNum < min then resultNum = min end
				if label then
					label.Text = text.. math.round(resultNum)
				end
				if event then result = resultNum end
				event(result)
			end
		end
	end)
	if not filling.Visible then filling.Visible = true end
end

game:GetService("CoreGui").Horizon.Settings.ActionSlideFrame.HealthRSliderFrame.HealthRSlider.Filling.Size = UDim2.new(50 / 100, 0, 1, 0)
game:GetService("CoreGui").Horizon.Settings.ActionSlideFrame.HealthRSliderFrame.HealthRSlider.HealthRLabel.Text = "50"
makeSlider(game:GetService("CoreGui").Horizon.Settings.ActionSlideFrame.HealthRSliderFrame.HealthRSlider, 10, 100, "X", 10, game:GetService("CoreGui").Horizon.Settings.ActionSlideFrame.HealthRSliderFrame.HealthRReset, game:GetService("CoreGui").Horizon.Settings.ActionSlideFrame.HealthRSliderFrame.HealthRSlider.HealthRLabel, "", function(result)
	if not result then result = 10 end
	result = math.round(result)
	if result ~= 0 and result < 10 then
		result = 10
	end
end)

