function daynight()
    if DayNight == true then
        while DayNight == true do
            if not game.CoreGui:FindFirstChild("Horizon") then break end
            task.wait()
            game:GetService("Lighting").TimeOfDay = "24:00:00"
            game:GetService("Lighting").ClockTime = 24
            game:GetService("Lighting").Brightness = 1
        end
    else
        while DayNight == false do
            if not game.CoreGui:FindFirstChild("Horizon") then break end
            task.wait()
            game:GetService("Lighting").TimeOfDay = "12:00:00"
            game:GetService("Lighting").ClockTime = 12
            game:GetService("Lighting").Brightness = 1
        end
    end
end

function blackballs()
    if DarkBalls == true then
        while DarkBalls == true do
            if not game.CoreGui:FindFirstChild("Horizon") then break end
            task.wait()
            for _, v in pairs(game.Workspace.Storage:GetChildren()) do
                if v.Name == "EnergySphere9" then
                    v.Transparency = 0
                    v.Color = Color3.fromRGB(0, 0, 0)
                end
            end
        end
    else
        DarkBalls = false
    end
end

function whitebullet()
    if LightBullet == true then
        while LightBullet == true do
            if not game.CoreGui:FindFirstChild("Horizon") then break end
            task.wait()
            for _, v in pairs(game.Workspace:GetChildren()) do
                if v.Name == "Part" then
                    v.Transparency = 0
                    v.Color = Color3.fromRGB(255, 255, 255)
                end
            end
        end
    else
        LightBullet = false
    end
end

function rgbrank()
    if RainbowRank == true then
        while RainbowRank == true do
            if not game.CoreGui:FindFirstChild("Horizon") then break end
            task.wait(0.1)
            PurpleRank = false
			local args = {[1] = {[1] = "ChangeRankEmblem",[2] = math.random(1,10)}}
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
        end
    elseif RainbowRank == false then
        RainbowRank = false
    end
end

function rgbweights()
    if RainbowWeights == true then
        while RainbowWeights == true do
            if not game.CoreGui:FindFirstChild("Horizon") then break end
            task.wait(0.1)
            PurpleRank = false
            local args = {[1] = {[1] = "Weight",[2] = "Weight"..math.random(1,4)}}
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
        end
    elseif RainbowWeights == false then
        RainbowWeights = false
    end
end

spawn(function()
	if SuperAura == true then
        SuperAura = true
        if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
            Horizon.SettingsButton3.BackgroundColor3 = Color3.fromRGB(69, 255, 113)
            Horizon.Interact3.ImageColor3 = Color3.fromRGB(10, 10, 10)
        else
            Horizon.Interact3.ImageColor3 = Color3.fromRGB(255, 255, 255)
            Horizon.SettingsButton3.BackgroundColor3 = Color3.fromRGB(144, 41, 246)
        end
    elseif SuperAura == false then
        SuperAura = false
        Horizon.Interact3.ImageColor3 = Color3.fromRGB(255, 255, 255)
        Horizon.SettingsButton3.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    end
end)

