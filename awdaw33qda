function LoadDefaults()
	RespawnActive = true
	DayNight = false
	AutoQuests = true
    SoundId = "rbxassetid://9043887091"
	SoundPlaying = false
	SoundTimePosition = 0
	SoundVolume = 0.1
	SuperAura = false
    NoAura = false
	RainbowRank = false
	RainbowWeights = false
	TrainJS = false
    TrainFSBR = false
    TrainPPBR = false
	DarkModeAutoExe = false
	LastPosition = false
    BeforeTrainPos = false
	DarkBalls = false
	LightBullet = false
    TranslatorAutoExe = false
	BetterEspAutoExe = false
	CmdXAutoExe = false
	DarkDexAutoExe = false
    MultiCheckAutoExe = false
    BlackHubAutoExe = false
    StackAutoExe = false
    IYAutoExec = false
end

function UpdateSaves()
    local Update = {
	RespawnActive = RespawnActive,
	DayNight = DayNight,
	AutoQuests = AutoQuests,
    SoundId = SoundId,
	SoundPlaying = SoundPlaying,
	SoundTimePosition = SoundTimePosition,
	SoundVolume = SoundVolume,
	SuperAura = SuperAura,
    NoAura = NoAura,
	RainbowRank = RainbowRank,
	RainbowWeights = RainbowWeights,
	TrainJS = TrainJS,
    TrainFSBR = TrainFSBR,
    TrainPPBR = TrainPPBR,
    DarkModeAutoExe = DarkModeAutoExe,
    LastPosition = LastPosition,
    BeforeTrainPos = BeforeTrainPos,
	DarkBalls = DarkBalls,
	LightBullet = LightBullet,
    TranslatorAutoExe = TranslatorAutoExe,
    BetterEspAutoExe = BetterEspAutoExe,
	CmdXAutoExe = CmdXAutoExe,
	DarkDexAutoExe = DarkDexAutoExe,
    MultiCheckAutoExe = MultiCheckAutoExe,
    BlackHubAutoExe = BlackHubAutoExe,
    StackAutoExe = StackAutoExe,
    IYAutoExec = IYAutoExec
    }
    writefile("HorizonUWP.lua", game:GetService("HttpService"):JSONEncode(Update))
end

function LoadSaves()
    local success, errorsend =
        pcall(
        function()
            saves = game:GetService("HttpService"):JSONDecode(readfile("HorizonUWP.lua"))
        end
    )
    if not success then
        LoadDefaults()
        UpdateSaves()
        return
    end
	RespawnActive = saves.RespawnActive
	DayNight = saves.DayNight
	AutoQuests = saves.AutoQuests
    SoundId = saves.SoundId
	SoundPlaying = saves.SoundPlaying
	SoundTimePosition = saves.SoundTimePosition
	SoundVolume = saves.SoundVolume
	SuperAura = saves.SuperAura
    NoAura = saves.NoAura
	RainbowRank = saves.RainbowRank
	RainbowWeights = saves.RainbowWeights
	TrainJS = saves.TrainJS
    TrainFSBR = saves.TrainFSBR
    TrainPPBR = saves.TrainPPBR
	DarkBalls = saves.DarkBalls
	LightBullet = saves.LightBullet
    BetterEspAutoExe = saves.BetterEspAutoExe
	CmdXAutoExe = saves.CmdXAutoExe
    MultiCheckAutoExe = saves.MultiCheckAutoExe
    BlackHubAutoExe = saves.BlackHubAutoExe
    TranslatorAutoExe = saves.TranslatorAutoExe
	DarkDexAutoExe = saves.DarkDexAutoExe
    DarkModeAutoExe = saves.DarkModeAutoExe
    StackAutoExe = saves.StackAutoExe
    IYAutoExec = saves.IYAutoExec
    LastPosition = saves.LastPosition
    BeforeTrainPos = saves.BeforeTrainPos
end

if writefile and readfile then
    LoadSaves()
else
    LoadDefaults()
end

