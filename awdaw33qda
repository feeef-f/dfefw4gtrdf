Horizon.PLAttackSoulAttack.Visible = false
Horizon.PLAttackSoulAttack.AutoButtonColor = false
Horizon.PLAttackSoulAttack.MouseButton1Click:Connect(function()
    if LoopSoulAttack == true then
        Horizon.PLAttackSoulAttackText.TextColor3 = Color3.fromRGB(238, 238, 238)
        Horizon.PLAttackSoulAttack.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        LoopSoulAttack = false
    elseif LoopSoulAttack == false then
        if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
            Horizon.PLAttackSoulAttackText.TextColor3 = Color3.fromRGB(10, 10, 10)
            Horizon.PLAttackSoulAttack.BackgroundColor3 = Color3.fromRGB(69, 255, 113)
        else
            Horizon.PLAttackSoulAttackText.TextColor3 = Color3.fromRGB(238, 238, 238)
            Horizon.PLAttackSoulAttack.BackgroundColor3 = Color3.fromRGB(144, 41, 246)
        end
        LoopSoulAttack = true
        while LoopSoulAttack == true do
            if not game.CoreGui:FindFirstChild("Horizon") then break end
            if LoopSoulAttack == false then break end
            task.wait()
            pcall(function()
                game.ReplicatedStorage.RemoteEvent:FireServer({"Skill_SoulAttack_Start", SelectedUser})
                game.ReplicatedStorage.RemoteEvent:FireServer({"Skill_SoulAttack_End"})
            end)
        end
    end
end)

