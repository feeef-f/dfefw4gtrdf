Horizon.PLAttackCBring.Name = "CBringButton"
Horizon.PLAttackCBring.Parent = Horizon.PLAttackFrame
Horizon.PLAttackCBring.Active = true
Horizon.PLAttackCBring.AnchorPoint = Vector2.new(0.5, 0.5)
Horizon.PLAttackCBring.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
Horizon.PLAttackCBring.BackgroundTransparency = 1
Horizon.PLAttackCBring.Position = UDim2.new(0.498, 0, 0.475, 0)
Horizon.PLAttackCBring.Selectable = true
Horizon.PLAttackCBring.Size = UDim2.new(0.9, 0, 0.17, 0)
Horizon.PLAttackCBring.Text = ""
Horizon.PLAttackCBring.BorderSizePixel = 0
Horizon.PLAttackCBring.Visible = false
Horizon.PLAttackCBring.AutoButtonColor = false
Horizon.PLAttackCBring.MouseButton1Click:Connect(function()
    if CBringing == true then
        Horizon.PLAttackCBringText.TextColor3 = Color3.fromRGB(238, 238, 238)
        Horizon.PLAttackCBring.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        CBringing = false
    elseif CBringing == false then
        if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
            Horizon.PLAttackCBringText.TextColor3 = Color3.fromRGB(10, 10, 10)
            Horizon.PLAttackCBring.BackgroundColor3 = Color3.fromRGB(69, 255, 113)
        else
            Horizon.PLAttackCBringText.TextColor3 = Color3.fromRGB(238, 238, 238)
            Horizon.PLAttackCBring.BackgroundColor3 = Color3.fromRGB(144, 41, 246)
        end
        CBringing = true
        while CBringing == true do
            if not game.CoreGui:FindFirstChild("Horizon") then break end
            if CBringing == false then break end
            task.wait()
            pcall(function()
                if SelectedUser ~= nil then
                    SelectedUser.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.lookVector * 1
                end
            end)
        end
    end
end)

