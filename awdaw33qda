Horizon.BarToggle.MouseButton1Click:connect(function()
    if BarHidden == false then
        BarHidden = true
        TweenServ:Create(Horizon.Bar, TweenInfo.new(1, Enum.EasingStyle.Exponential), {Position = UDim2.new(0.5, 0, 1.05, 0)}):Play()
        TweenServ:Create(Horizon.BarToggle, TweenInfo.new(1, Enum.EasingStyle.Exponential), {Position = UDim2.new(0.5, 0, 0.98, 0)}):Play()
        TweenServ:Create(Horizon.BarToggle, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {Rotation = 180}):Play()
    else
        BarHidden = false
        TweenServ:Create(Horizon.Bar, TweenInfo.new(1, Enum.EasingStyle.Exponential), {Position = UDim2.new(0.5, 0, 0.948, 0)}):Play()
        TweenServ:Create(Horizon.BarToggle, TweenInfo.new(1, Enum.EasingStyle.Exponential), {Position = UDim2.new(0.5, 0, 0.895, 0)}):Play()
        TweenServ:Create(Horizon.BarToggle, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {Rotation = 0}):Play()
    end
end)

spawn(function()
    while task.wait() do
        if not game.CoreGui:FindFirstChild("Horizon") then break end
        pcall(function()
            if getgenv().stack ~= true then
                if game.Players.LocalPlayer.Character then
                    olderposition = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                end
            end
        end)
    end
end)

spawn(function()
    while task.wait(1) do
        if not game.CoreGui:FindFirstChild("Horizon") then break end
        getgenv().rejoin = game:GetService("CoreGui").RobloxPromptGui.promptOverlay.ChildAdded:Connect(function(child)
            if child.Name == 'ErrorPrompt' and child:FindFirstChild('MessageArea') and child.MessageArea:FindFirstChild("ErrorFrame") then
                if getgenv().stack ~= true then
                    LastPosition = {["Pos"] = table.concat({olderposition:GetComponents()},",")}
                    if TrainFS == true then
                        TrainFSBR = true
                        BeforeTrainPos = {["Pos"] = table.concat({bftposition:GetComponents()},",")}
                    elseif TrainPP == true then
                        BeforeTrainPos = {["Pos"] = table.concat({bftposition:GetComponents()},",")}
                        TrainFSBR = true
                    end
                    UpdateSaves()
                end
                pcall(game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, game.JobId))
            end
        end)
    end
end)

function Format(Int)
	return string.format("%02i", Int)
end

function convertToHMS(Seconds)
	local Minutes = (Seconds - Seconds%60)/60
	Seconds = Seconds - Minutes*60
	local Hours = (Minutes - Minutes%60)/60
	Minutes = Minutes - Hours*60	
	
	if Hours ~= 0 then
		return Format(Hours)..":"..Format(Minutes)..":"..Format(Seconds)
	else
		return Format(Minutes)..":"..Format(Seconds)
	end
end

function CheckWritefile()
	if isfile and delfile and writefile and readfile then
		return true
	end
	return false
end

function UpdateExecutor()
	if not identifyexecutor then
		Client = "Universal"
	else
		Client = identifyexecutor()
	end
end
UpdateExecutor()

spawn(function()
    while task.wait() do
        if not game.CoreGui:FindFirstChild("Horizon") then break end
        Horizon.BigPage.Data.data.Executor.Text = "Executor: "..Client..""
        Horizon.BigPage.Data.data.Version.Text = "Version: "..Release..""
        Horizon.BigPage.Data.data.Time.Text = "Session Time: "..convertToHMS(elapsedTime())..""
        Horizon.BigPage.Data.data.Players.Text = "Players: "..tostring(#game.Players:GetChildren()).."/"..tostring(game.Players.MaxPlayers)..""
        Horizon.BigPage.Data.data.Ping.Text = "Ping: "..math.round(game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue()).." ms"
        if not CheckWritefile() then
            Horizon.BigPage.Data.data.File.Text = "Horizon Data File: Disabled By Executor"
        else
            Horizon.BigPage.Data.data.File.Text = "Horizon Data File: Loaded"
        end
    end
end)

-- Finishing

HasFullyExecuted = true
Horizon.Bar.Visible = true

spawn(function()
TweenServ:Create(game:GetService("CoreGui").RobloxGui.Backpack, TweenInfo.new(1, Enum.EasingStyle.Elastic), {Position = UDim2.new(0.2, 0 , -0.965, 0)}):Play()
TweenServ:Create(Horizon.Bar, TweenInfo.new(1, Enum.EasingStyle.Exponential), {Position = UDim2.new(0.5, 0, 0.948, 0)}):Play()
TweenServ:Create(Horizon.Loading, TweenInfo.new(1, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.5}):Play()
Horizon.LoadingText.Text = "Loading Playerlist."

repeat task.wait() until pListCreated == true

spawn(function()
    game:GetService("Players").PlayerAdded:Connect(function(whojoined)
        repeat task.wait() until HasFullyExecuted == true
        if whojoined.Name == "CaughtInTheAbyss" then
            NewNotification("Abyss has joined the server.", 6, "rainbow")
        elseif whojoined.Name == "CaughtInTheAbyss_Alt" or whojoined.Name == "CaughtInTheAbyss_Alt1" then
            NewNotification("Abyss Alt has joined the server.", 6, "rainbow")
        elseif whojoined:IsFriendsWith(game.Players.LocalPlayer.UserId) then
            NewNotification(""..whojoined.Name.." joined the server.", 6, "purple")
        else
            NewNotification(""..whojoined.Name.." joined the server.", 6, "green")
        end
    end)
end)

local SaveName

spawn(function()
    game:GetService("Players").PlayerRemoving:Connect(function(wholeft)
        repeat task.wait() until HasFullyExecuted == true
        SaveName = wholeft.Name
        NewNotification(""..SaveName.." left the server.", 6, "red")
        spawn(function()
            if SelectedUser ~= nil then
                if SaveName == SelectedUser.Name then
                    SelectedUser = nil
                    StatsShowing = false
                    if CBringing == true then
                        CBringing = false
                        Horizon.PLAttackCBring.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
                    elseif LoopReap == true then
                        LoopReap = false
                        Horizon.PLAttackReap.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
                    elseif LoopSoulAttack == true then
                        LoopSoulAttack = false
                        Horizon.PLAttackSoulAttack.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
                    end
                end
            end
        end)
    end)
end)

spawn(function()
	while task.wait() do
        if not game.CoreGui:FindFirstChild("Horizon") then break end
        pcall(function()
            for i, child in pairs(Horizon.Apps.PlayerList:GetChildren()) do
                if child:IsA("Frame") then
                    if child.Name ~= "shittop" then
                        if child.Name ~= "shitbottom" then
                            if string.match(string.lower(child.Name), string.lower(peopleSearchBar.Text)) then
                                child.Visible = true
                            elseif string.match(string.lower(game.Players[child.Name].DisplayName), string.lower(peopleSearchBar.Text)) then
                                child.Visible = true
                            else
                                child.Visible = false
                            end
                        end
                    end
                end
                if lastSearch ~= peopleSearchBar.Text then
                    Horizon.Apps.PlayerList.CanvasPosition = Vector2.new(0,0)
                end
                lastSearch = peopleSearchBar.Text
            end
        end)
	end
end)

