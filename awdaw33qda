    Horizon.LoadingIcon.Rotation = 0
    TweenServ:Create(Horizon.LoadingIcon, TweenInfo.new(1,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,200,false,0), {Rotation = 360}):Play()
end)

