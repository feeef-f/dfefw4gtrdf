Horizon.PLAttackReap.Name = "ReapButton"
Horizon.PLAttackReap.Parent = Horizon.PLAttackFrame
Horizon.PLAttackReap.Active = true
Horizon.PLAttackReap.AnchorPoint = Vector2.new(0.5, 0.5)
Horizon.PLAttackReap.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
Horizon.PLAttackReap.BackgroundTransparency = 1
Horizon.PLAttackReap.Position = UDim2.new(0.498, 0, 0.678, 0)
Horizon.PLAttackReap.Selectable = true
Horizon.PLAttackReap.Size = UDim2.new(0.9, 0, 0.17, 0)
Horizon.PLAttackReap.Text = ""
Horizon.PLAttackReap.BorderSizePixel = 0
Horizon.PLAttackReap.Visible = false
Horizon.PLAttackReap.AutoButtonColor = false
Horizon.PLAttackReap.MouseButton1Click:Connect(function()
    if LoopReap == true then
        Horizon.PLAttackReapText.TextColor3 = Color3.fromRGB(238, 238, 238)
        Horizon.PLAttackReap.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        LoopReap = false
    elseif LoopReap == false then
        if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
            Horizon.PLAttackReapText.TextColor3 = Color3.fromRGB(10, 10, 10)
            Horizon.PLAttackReap.BackgroundColor3 = Color3.fromRGB(69, 255, 113)
        else
            Horizon.PLAttackReapText.TextColor3 = Color3.fromRGB(238, 238, 238)
            Horizon.PLAttackReap.BackgroundColor3 = Color3.fromRGB(144, 41, 246)
        end
        LoopReap = true
        while LoopReap == true do
            if not game.CoreGui:FindFirstChild("Horizon") then break end
            if LoopReap == false then break end
            task.wait(0.2)
            pcall(function()
                game.ReplicatedStorage.RemoteEvent:FireServer({"Skill_SoulReap", SelectedUser})
            end)
        end
    end
end)

