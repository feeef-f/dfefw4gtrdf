        daynight()
    elseif DayNight == true then
        Horizon.Interact8.ImageColor3 = Color3.fromRGB(255, 255, 255)
        Horizon.SettingsButton8.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        DayNight = false
        daynight()
    end
end)

local goddedtime = math.huge

Horizon.Interact9.MouseButton1Click:connect(function()
    spawn(function()
        pcall(function()
            Godmoded = true
            game.Players.LocalPlayer.Character:WaitForChild("GodModeShield", goddedtime)
            Godmoded = false
        end)
    end)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CaughtInTheAbyss/Secret/main/God", true))()
    if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
        Horizon.SettingsButton9.BackgroundColor3 = Color3.fromRGB(69, 255, 113)
        Horizon.Interact9.ImageColor3 = Color3.fromRGB(10, 10, 10)
    else
        Horizon.Interact9.ImageColor3 = Color3.fromRGB(255, 255, 255)
        Horizon.SettingsButton9.BackgroundColor3 = Color3.fromRGB(144, 41, 246)
    end
    task.wait(1)
    Horizon.Interact9.ImageColor3 = Color3.fromRGB(255, 255, 255)
    Horizon.SettingsButton9.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
end)

Horizon.Interact10.MouseButton1Click:connect(function()
    NewNotification("Joining a new server for you.", 10, "")
    if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
        Horizon.SettingsButton10.BackgroundColor3 = Color3.fromRGB(69, 255, 113)
        Horizon.Interact10.ImageColor3 = Color3.fromRGB(10, 10, 10)
    else
        Horizon.Interact10.ImageColor3 = Color3.fromRGB(255, 255, 255)
        Horizon.SettingsButton10.BackgroundColor3 = Color3.fromRGB(144, 41, 246)
    end
    local char = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")
    LastPosition = {["Pos"] = table.concat({hrp.CFrame:GetComponents()},",")}
    UpdateSaves()
    local GUIDs = {}
    local maxPlayers = 0
    local pagesToSearch = 5
    local Http = game:GetService("HttpService"):JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Asc&limit=100&cursor="))
    for i = 1,pagesToSearch do
        for i,v in pairs(Http.data) do
            if v.playing ~= v.maxPlayers and v.id ~= game.JobId then
                maxPlayers = v.maxPlayers
                table.insert(GUIDs, {id = v.id, users = v.playing}):Play()
            end
        end
        if Http.nextPageCursor ~= null then Http = game:GetService("HttpService"):JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Asc&limit=100&cursor="..Http.nextPageCursor)) else break end
    end
    game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, GUIDs[math.random(1,#GUIDs)].id, game.Players.LocalPlayer)
end)

Horizon.Interact10.MouseButton2Click:connect(function()
    pcall(function()
        NewNotification("Rejoining the server for you.", 10, "")
        local char = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart")
        LastPosition = {["Pos"] = table.concat({hrp.CFrame:GetComponents()},",")}
        if TrainFS == true then
            TrainFSBR = true
            BeforeTrainPos = {["Pos"] = table.concat({bftposition:GetComponents()},",")}
        elseif TrainPP == true then
            BeforeTrainPos = {["Pos"] = table.concat({bftposition:GetComponents()},",")}
            TrainFSBR = true
        end
        UpdateSaves()
        if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
            Horizon.SettingsButton10.BackgroundColor3 = Color3.fromRGB(69, 255, 113)
            Horizon.Interact10.ImageColor3 = Color3.fromRGB(10, 10, 10)
        else
            Horizon.Interact10.ImageColor3 = Color3.fromRGB(255, 255, 255)
            Horizon.SettingsButton10.BackgroundColor3 = Color3.fromRGB(144, 41, 246)
        end
        game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, game.JobId)
    end)
end)

Horizon.Interact11.MouseButton1Click:connect(function()
    if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
        Horizon.SettingsButton11.BackgroundColor3 = Color3.fromRGB(69, 255, 113)
        Horizon.Interact11.ImageColor3 = Color3.fromRGB(10, 10, 10)
    else
        Horizon.Interact11.ImageColor3 = Color3.fromRGB(255, 255, 255)
        Horizon.SettingsButton11.BackgroundColor3 = Color3.fromRGB(144, 41, 246)
    end
    loadstring(game:HttpGet('https://raw.githubusercontent.com/CaughtInTheAbyss/Secret/main/nologs', true))()
    task.wait(0.5)
    Horizon.Interact11.ImageColor3 = Color3.fromRGB(255, 255, 255)
    Horizon.SettingsButton11.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
end)

Horizon.Interact12.MouseButton1Click:connect(function()
    if DarkBalls == false then
        if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
            Horizon.SettingsButton12.BackgroundColor3 = Color3.fromRGB(69, 255, 113)
            Horizon.Interact12.ImageColor3 = Color3.fromRGB(10, 10, 10)
        else
            Horizon.Interact12.ImageColor3 = Color3.fromRGB(255, 255, 255)
            Horizon.SettingsButton12.BackgroundColor3 = Color3.fromRGB(144, 41, 246)
        end
        DarkBalls = true
        blackballs()
    elseif DarkBalls == true then
        Horizon.Interact12.ImageColor3 = Color3.fromRGB(255, 255, 255)
        Horizon.SettingsButton12.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        DarkBalls = false
    end
end)

Horizon.Interact13.MouseButton1Click:connect(function()
    if LightBullet == false then
        if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
            Horizon.SettingsButton13.BackgroundColor3 = Color3.fromRGB(69, 255, 113)
            Horizon.Interact13.ImageColor3 = Color3.fromRGB(10, 10, 10)
        else
            Horizon.Interact13.ImageColor3 = Color3.fromRGB(255, 255, 255)
            Horizon.SettingsButton13.BackgroundColor3 = Color3.fromRGB(144, 41, 246)
        end
        LightBullet = true
        whitebullet()
    elseif LightBullet == true then
        Horizon.Interact13.ImageColor3 = Color3.fromRGB(255, 255, 255)
        Horizon.SettingsButton13.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        LightBullet = false
    end
end)

MusicTable.SongName.Text = game:GetService("MarketplaceService"):GetProductInfo(tostring(string.gsub(MusicTable.LMusic.SoundId, "rbxassetid://", ""))).Name
game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack({[1] = {[1] = "Setting",[2] = "BGMusic",[3] = false}}))

makeSlider(MusicTable.Progress, 0, 1, "X", 0, nil, nil, nil, function(result)
	if not result then result = 0 end
	MusicTable.LMusic.TimePosition = result * MusicTable.LMusic.TimeLength
end)

makeSlider(MusicTable.Volume, 0, 1, "Y", 0.1, nil, nil, nil, function(result)
	if not result then result = 0.1 end
	MusicTable.LMusic.Volume = result
	SoundVolume = result
end)

spawn(function()
	while task.wait(0.1) do
        if not game.CoreGui:FindFirstChild("Horizon") then break end
		if Horizon.BigPage:FindFirstChild("Music") then
			MusicTable.Filling_2.Size = UDim2.new(MusicTable.LMusic.TimePosition / MusicTable.LMusic.TimeLength, 0, 1, 0)
			MusicTable.Filling.Size = UDim2.new(1, 0, MusicTable.LMusic.Volume / 1)
		end
	end
end)

MusicTable.Button.MouseButton1Click:Connect(function()
	if MusicTable.LMusic.Playing then
		MusicTable.LMusic.Playing = false
		MusicTable.PlayIcon.Visible = true
		MusicTable.PauseIcon.Visible = false
	else
		game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack({[1] = {[1] = "Setting",[2] = "BGMusic",[3] = false}}))
		MusicTable.LMusic.Playing = true
		MusicTable.PlayIcon.Visible = false
		MusicTable.PauseIcon.Visible = true
	end
end)

MusicTable.Button_2.MouseButton1Click:Connect(function()
	game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack({[1] = {[1] = "Setting",[2] = "BGMusic",[3] = false}}))
	local asset = MusicTable.AssetBox
	MusicTable.LMusic.SoundId = "rbxassetid://"..tonumber(asset.Text)
	MusicTable.SongName.Text = game:GetService("MarketplaceService"):GetProductInfo(tostring(string.gsub(MusicTable.LMusic.SoundId, "rbxassetid://", ""))).Name
	asset.Text = ""
	MusicTable.LMusic.Playing = true
	MusicTable.LMusic.TimePosition = 0
	MusicTable.PlayIcon.Visible.Visible = false
	MusicTable.PauseIcon.Visible = true
end)

spawn(function()
	while task.wait(1) do
        if not game.CoreGui:FindFirstChild("Horizon") then break end
        repeat task.wait() until HasFullyExecuted == true
		SoundId = MusicTable.LMusic.SoundId
		SoundPlaying = MusicTable.LMusic.Playing
		SoundTimePosition = MusicTable.LMusic.TimePosition
	end
end)

spawn(function()
    MusicTable.LMusic.Playing = SoundPlaying
end)

local stillhovering = false

function hoverInfo(button, infoText, stillhover)
	local infoEl = Horizon.HoverElement
	
	button.MouseEnter:Connect(function()
        stillhover = true
        task.wait(.5)
        if stillhover == true then
		infoEl.Visible = true
		infoEl.Background.InfoLabel.Text = infoText
		infoEl.Position = UDim2.new(0, button.AbsolutePosition.X + (button.AbsoluteSize.X / 2), 0, button.AbsolutePosition.Y)
        end
	end)
	button.MouseLeave:Connect(function()
        stillhover = false
		infoEl.Visible = false
	end)
end

local fiststat = game.Players.LocalPlayer.PrivateStats.FistStrength.Value
local bodystat = game.Players.LocalPlayer.PrivateStats.BodyToughness.Value
local psychicstat = game.Players.LocalPlayer.PrivateStats.PsychicPower.Value

function hoverInfo1(button, color)
	local infoEl1 = Horizon.HoverElement:Clone()
    infoEl1.Parent = Horizon.Horizon
	if not color then color = infoEl1.Background.BackgroundColor3 end

    spawn(function()
        while task.wait(0.1) do
            if not game.CoreGui:FindFirstChild("Horizon") then break end
            infoEl1.Background.InfoLabel.Text = "Body: +"..api.suffixstring(game.Players.LocalPlayer.PrivateStats.BodyToughness.Value - bodystat)
        end
    end)
	
	Horizon.BtBg.MouseEnter:Connect(function()
		infoEl1.Visible = true
		infoEl1.Position = UDim2.new(0, button.AbsolutePosition.X + (button.AbsoluteSize.X / 2), 0, button.AbsolutePosition.Y)
	end)
	Horizon.BtBg.MouseLeave:Connect(function()
		infoEl1.Visible = false
	end)
end

function hoverInfo2(button, color)
	local infoEl2 = Horizon.HoverElement:Clone()
    infoEl2.Parent = Horizon.Horizon
	if not color then color = infoEl2.Background.BackgroundColor3 end

    spawn(function()
        while task.wait(0.1) do
            if not game.CoreGui:FindFirstChild("Horizon") then break end
            infoEl2.Background.InfoLabel.Text = "Fist: +"..api.suffixstring(game.Players.LocalPlayer.PrivateStats.FistStrength.Value - fiststat)
        end
    end)
	
	Horizon.FistBg.MouseEnter:Connect(function()
		infoEl2.Visible = true
		infoEl2.Position = UDim2.new(0, button.AbsolutePosition.X + (button.AbsoluteSize.X / 2), 0, button.AbsolutePosition.Y)
	end)
	Horizon.FistBg.MouseLeave:Connect(function()
		infoEl2.Visible = false
    end)
end

function hoverInfo3(button, color)
	local infoEl3 = Horizon.HoverElement:Clone()
    infoEl3.Parent = Horizon.Horizon
	if not color then color = infoEl3.Background.BackgroundColor3 end
	
