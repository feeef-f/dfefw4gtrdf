    if TrainPPDouble == true then return end
    TrainFSDouble = not TrainFSDouble
    if TrainFSDouble then
        PrepareTrain()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-279, 279, 1007)
        if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
            Horizon.ExperimentalSettings1Title.TextColor3 = Color3.fromRGB(10, 10, 10)
            Horizon.ExperimentalSettings1.BackgroundColor3 = Color3.fromRGB(69, 255, 113)
        else
            Horizon.ExperimentalSettings1Title.TextColor3 = Color3.fromRGB(255, 255, 255)
            Horizon.ExperimentalSettings1.BackgroundColor3 = Color3.fromRGB(144, 41, 246)
        end
    else
        setfpscap(999)
        settings().Rendering.QualityLevel = SavedQualityLevel
        PreTrain = false
        Horizon.ExperimentalSettings1Title.TextColor3 = Color3.fromRGB(255, 255, 255)
		Horizon.ExperimentalSettings1.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        RespawnAP()
        oldposition = bftposition
        bftposition = nil
    end
end)

