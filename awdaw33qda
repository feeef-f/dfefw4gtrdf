    TweenServ:Create(Horizon.SettingsButton2, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    TweenServ:Create(Horizon.SettingsButton3, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    TweenServ:Create(Horizon.SettingsButton4, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    TweenServ:Create(Horizon.SettingsButton5, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    TweenServ:Create(Horizon.SettingsButton6, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    TweenServ:Create(Horizon.SettingsButton7, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    TweenServ:Create(Horizon.SettingsButton8, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    TweenServ:Create(Horizon.SettingsButton9, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    TweenServ:Create(Horizon.SettingsButton10, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    TweenServ:Create(Horizon.SettingsButton11, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    TweenServ:Create(Horizon.SettingsButton12, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    TweenServ:Create(Horizon.SettingsButton13, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    TweenServ:Create(Horizon.Interact2, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
    TweenServ:Create(Horizon.Interact3, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
    TweenServ:Create(Horizon.Interact4, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
    TweenServ:Create(Horizon.Interact5, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
    TweenServ:Create(Horizon.Interact6, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
    TweenServ:Create(Horizon.Interact7, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
    TweenServ:Create(Horizon.Interact8, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
    TweenServ:Create(Horizon.Interact9, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
    TweenServ:Create(Horizon.Interact10, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
    TweenServ:Create(Horizon.Interact11, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
    TweenServ:Create(Horizon.Interact12, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
    TweenServ:Create(Horizon.Interact13, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
    TweenServ:Create(Horizon.HealthRFilling, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    TweenServ:Create(Horizon.HealthRSlider, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.8}):Play()
    TweenServ:Create(Horizon.HealthRLabel, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
    TweenServ:Create(Horizon.HealthRLabel1, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
    TweenServ:Create(Horizon.Script7, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    TweenServ:Create(Horizon.Script7Title, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
    TweenServ:Create(Horizon.Script8, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    TweenServ:Create(Horizon.Script8Title, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
    TweenServ:Create(Horizon.Script9, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    TweenServ:Create(Horizon.Script9Title, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
    TweenServ:Create(Horizon.Shadow, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0.75}):Play() 
end

function CloseSettings()
    SettingsOpen = false
    if GlowOn then
        GlowOn = false
        TweenServ:Create(Horizon.BarMover, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {BackgroundTransparency = 1}):Play()
    end
    Horizon.BarToggle.Visible = true
    if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
        TweenServ:Create(Horizon.Button4Icon, TweenInfo.new(0.4, Enum.EasingStyle.Exponential), {ImageColor3 = Color3.fromRGB(255, 255, 255)}):Play()
    end
    TweenServ:Create(Horizon.Settings, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.SettingsLine, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.SettingsTitle, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
    TweenServ:Create(Horizon.SettingsTitleImage, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
    TweenServ:Create(Horizon.SettingsButton2, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.SettingsButton3, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.SettingsButton4, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.SettingsButton5, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.SettingsButton6, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.SettingsButton7, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.SettingsButton8, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.SettingsButton9, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.SettingsButton10, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.SettingsButton11, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.SettingsButton12, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.SettingsButton13, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.Interact2, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
    TweenServ:Create(Horizon.Interact3, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
    TweenServ:Create(Horizon.Interact4, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
    TweenServ:Create(Horizon.Interact5, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
    TweenServ:Create(Horizon.Interact6, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
    TweenServ:Create(Horizon.Interact7, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
    TweenServ:Create(Horizon.Interact8, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
    TweenServ:Create(Horizon.Interact9, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
    TweenServ:Create(Horizon.Interact10, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
    TweenServ:Create(Horizon.Interact11, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
    TweenServ:Create(Horizon.Interact12, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
    TweenServ:Create(Horizon.Interact13, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
    TweenServ:Create(Horizon.HealthRFilling, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.HealthRSlider, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.HealthRLabel, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
    TweenServ:Create(Horizon.HealthRLabel1, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
    TweenServ:Create(Horizon.Script7, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.Script7Title, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
    TweenServ:Create(Horizon.Script8, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.Script8Title, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
    TweenServ:Create(Horizon.Script9, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.Script9Title, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
    TweenServ:Create(Horizon.Shadow, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
    Horizon.Interact2.Visible = false
    Horizon.Interact3.Visible = false
    Horizon.Interact4.Visible = false
    Horizon.Interact5.Visible = false
    Horizon.Interact6.Visible = false
    Horizon.Interact7.Visible = false
    Horizon.Interact8.Visible = false
    Horizon.Interact9.Visible = false
    Horizon.Interact10.Visible = false
    Horizon.Interact11.Visible = false
    Horizon.Interact12.Visible = false
    Horizon.Interact13.Visible = false
    Horizon.Script7.Visible = false
    Horizon.Script8.Visible = false
    Horizon.Script9.Visible = false
    Horizon.HealthRSlider.Visible = false
    Horizon.SettingsTitleImage.Visible = false
    Horizon.ExperimentalSettingsFrame.Visible = false
end

Horizon.Button1.MouseButton1Down:connect(function()
    if HomeOpen == false then
        OpenHome()
    else
        CloseHome()
    end
end)

Horizon.Button2.MouseButton1Down:connect(function()
    if AppsOpen == false then
        OpenApps()
    else
        CloseApps()
    end
end)

Horizon.Button3.MouseButton1Down:connect(function()
    if ScriptsOpen == false then
        OpenScripts()
    else
        CloseScripts()
    end
end)

Horizon.Button4.MouseButton1Down:connect(function()
    if SettingsOpen == false then
        OpenSettings()
    else
        CloseSettings()
    end
end)

repeat task.wait() until game:GetService("Players").LocalPlayer.Character

spawn(function()
    function createPlayerList()
        repeat task.wait() until HasFullyExecuted == true
        for i, plr in pairs(game.Players:GetPlayers()) do
            local ui = Horizon.AttackFrame:Clone()
            ui.Name = plr.Name
            ui.Parent = Horizon.Apps.PlayerList
            ui.NameDisplay.Username.Text = plr.Name
            ui.UserIcon.Image = game.Players:GetUserThumbnailAsync(plr.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
            Horizon.Apps.PlayerList.UIListLayout.Padding = UDim.new(0, 5)
            Horizon.Apps.PlayerList.CanvasSize = UDim2.new(0, 0, 0, Horizon.Apps.PlayerList.UIListLayout.AbsoluteContentSize.Y + 2.5)
            repeat task.wait() until plr.PrivateStats
            if plr.Name == "CaughtInTheAbyss" then
                ui.PremiumBar.Text.Text = "Owner/GigaChad"
                ui.PremiumBar.Text.TextColor3 = Color3.fromRGB(0, 0, 0)
                ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                ui.PremiumBar.Visible = true
                local speed = 5
                spawn(function()
                    while true do
                        if not game.CoreGui:FindFirstChild("Horizon") then break end
                        for i = 0,1,0.001*speed do
                            ui.PremiumBar.BackgroundColor3 = Color3.fromHSV(i,1,1)
                            wait()
                        end
                    end
                end)           
            elseif plr.Name == "CaughtInTheAbyss_Alt" then
                ui.PremiumBar.Text.Text = "Abyss"
                ui.PremiumBar.Text.TextColor3 = Color3.fromRGB(0, 0, 0)
                ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(100, 255, 100)
                ui.PremiumBar.Visible = true
                local speed = 5
                spawn(function()
                    while true do
                        if not game.CoreGui:FindFirstChild("Horizon") then break end
                        for i = 0,1,0.001*speed do
                            ui.PremiumBar.BackgroundColor3 = Color3.fromHSV(i,1,1)
                            wait()
                        end
                    end
                end)
            elseif plr.Name == "Quantavio_alt3" then
                ui.PremiumBar.Text.Text = "Quan"
                ui.PremiumBar.Text.TextColor3 = Color3.fromRGB(0, 0, 0)
                ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(100, 255, 100)
                ui.PremiumBar.Visible = true
                local speed = 5
                spawn(function()
                    while true do
                        if not game.CoreGui:FindFirstChild("Horizon") then break end
                        for i = 0,1,0.001*speed do
                            ui.PremiumBar.BackgroundColor3 = Color3.fromHSV(i,1,1)
                            wait()
                        end
                    end
                end)
            elseif plr.Name == "xqDerpy" then
                ui.PremiumBar.Text.Text = "Special Derp"
                ui.PremiumBar.Text.TextColor3 = Color3.fromRGB(0, 0, 0)
                ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(255, 215, 0)
                ui.PremiumBar.Visible = true
            elseif plr.Name == "oOFROZEN" then
                ui.PremiumBar.Text.Text = "GigaChad"
                ui.PremiumBar.Text.TextColor3 = Color3.fromRGB(0, 0, 0)
                ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                ui.PremiumBar.Visible = true
            elseif plr.Name == "Sinfliction" then
                ui.PremiumBar.Text.Text = ">:3"
                ui.PremiumBar.Text.TextColor3 = Color3.fromRGB(0, 0, 0)
                ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                ui.PremiumBar.Visible = true
            elseif plr.Name == "IITheReal_JoeyAltII" then
                ui.PremiumBar.Text.Text = "Special Joey"
                ui.PremiumBar.Text.TextColor3 = Color3.fromRGB(0, 0, 0)
                ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(5, 254, 159)
                ui.PremiumBar.Visible = true
            elseif plr.Name == "IITheReal_JoeyII" then
                ui.PremiumBar.Text.Text = "Special Joey"
                ui.PremiumBar.Text.TextColor3 = Color3.fromRGB(0, 0, 0)
                ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(5, 254, 159)
                ui.PremiumBar.Visible = true
            elseif plr.PrivateStats.FistStrength.Value >= game.Players.LocalPlayer.PrivateStats.BodyToughness.Value * .70 then
                ui.PremiumBar.Text.Text = "Fist Threat"
                ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
                ui.PremiumBar.Visible = true
            elseif plr.PrivateStats.PsychicPower.Value >= game.Players.LocalPlayer.PrivateStats.PsychicPower.Value * 10 then
                ui.PremiumBar.Text.Text = "Psychic Threat"
                ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(50, 50, 255)
                ui.PremiumBar.Visible = true
            end
            ui.UserIcon.Image = game.Players:GetUserThumbnailAsync(plr.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
            ui.StatsBtn.MouseButton1Click:connect(function()
            if SelectedUser ~= plr then
                SelectedUser = plr
                if SelectedUser then
                    StatsShowing = true
                    Horizon.PLStatsFistTxt.Text = "Fist:\n"..converttoletter(SelectedUser.PrivateStats.FistStrength.Value)
                    Horizon.PLStatsBodyTxt.Text = "Body:\n"..converttoletter(SelectedUser.PrivateStats.BodyToughness.Value)
                    Horizon.PLStatsPsyTxt.Text = "Psychic:\n"..converttoletter(SelectedUser.PrivateStats.PsychicPower.Value)
                    TweenServ:Create(Horizon.PLStatsFrame, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.1}):Play()
                    TweenServ:Create(Horizon.PLStatLine, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
                    TweenServ:Create(Horizon.PLStatsFist, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.8}):Play()
                    TweenServ:Create(Horizon.PLStatsBody, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.8}):Play()
                    TweenServ:Create(Horizon.PLStatsPsy, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.8}):Play()
                    TweenServ:Create(Horizon.PLSTitle, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                    TweenServ:Create(Horizon.PLStatsFistTxt, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                    TweenServ:Create(Horizon.PLStatsBodyTxt, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                    TweenServ:Create(Horizon.PLStatsPsyTxt, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                    TweenServ:Create(Horizon.PLSTitleImage, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
                    TweenServ:Create(Horizon.PLStatsFrameShadow, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0.75}):Play()
                    Horizon.PLAttackTeleport.Visible = true
                    Horizon.PLAttackCBring.Visible = true
                    Horizon.PLAttackReap.Visible = true
                    Horizon.PLAttackSoulAttack.Visible = true
                    TweenServ:Create(Horizon.PLAttackFrame, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.1}):Play()
                    TweenServ:Create(Horizon.PLAttackLine, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
                    TweenServ:Create(Horizon.PLAttackTitle, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                    TweenServ:Create(Horizon.PLAttackTitleImage, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
                    TweenServ:Create(Horizon.PLAttackFrameShadow, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0.75}):Play()
                    TweenServ:Create(Horizon.PLAttackTeleport, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
                    TweenServ:Create(Horizon.PLAttackCBring, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
                    TweenServ:Create(Horizon.PLAttackReap, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
                    TweenServ:Create(Horizon.PLAttackSoulAttack, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
                    TweenServ:Create(Horizon.PLAttackTeleportText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                    TweenServ:Create(Horizon.PLAttackCBringText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                    TweenServ:Create(Horizon.PLAttackReapText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                    TweenServ:Create(Horizon.PLAttackSoulAttackText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                    Horizon.PLAttackCBring.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
                    Horizon.PLAttackReap.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
                    Horizon.PLAttackSoulAttack.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
                    CBringing = false
                    LoopReap = false
                    LoopSoulAttack = false
                end
            elseif SelectedUser == plr then
                StatsShowing = false
                TweenServ:Create(Horizon.PLStatsFrame, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLStatLine, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLStatsFist, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLStatsBody, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLStatsPsy, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLSTitle, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLStatsFistTxt, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLStatsBodyTxt, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLStatsPsyTxt, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLSTitleImage, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLStatsFrameShadow, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
                Horizon.PLAttackTeleport.Visible = false
                Horizon.PLAttackCBring.Visible = false
                Horizon.PLAttackReap.Visible = false
                Horizon.PLAttackSoulAttack.Visible = false
                TweenServ:Create(Horizon.PLAttackFrame, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLAttackLine, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLAttackTitle, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLAttackTitleImage, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLAttackFrameShadow, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLAttackTeleport, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLAttackCBring, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLAttackReap, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLAttackSoulAttack, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLAttackTeleportText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLAttackCBringText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLAttackReapText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
                TweenServ:Create(Horizon.PLAttackSoulAttackText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
                SelectedUser = nil
            end
           end)
        end
        pListCreated = true
    end
    createPlayerList()
end)
spawn(function()
game.Players.PlayerAdded:Connect(function(plr)
    repeat task.wait() until HasFullyExecuted == true
	if pListCreated then
        local ui = Horizon.AttackFrame:Clone()
        ui.Name = plr.Name
        ui.Parent = Horizon.Apps.PlayerList
        ui.NameDisplay.Username.Text = plr.Name
        ui.UserIcon.Image = game.Players:GetUserThumbnailAsync(plr.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
        Horizon.Apps.PlayerList.UIListLayout.Padding = UDim.new(0, 5)
        Horizon.Apps.PlayerList.CanvasSize = UDim2.new(0, 0, 0, Horizon.Apps.PlayerList.UIListLayout.AbsoluteContentSize.Y + 2.5)
        repeat task.wait() until plr.PrivateStats
        if plr.Name == "CaughtInTheAbyss" then
            ui.PremiumBar.Text.Text = "Owner/GigaChad"
            ui.PremiumBar.Text.TextColor3 = Color3.fromRGB(0, 0, 0)
            ui.PremiumBar.Visible = true
            local speed = 5
            spawn(function()
                while true do
                    if not game.CoreGui:FindFirstChild("Horizon") then break end
                    for i = 0,1,0.001*speed do
                        ui.PremiumBar.BackgroundColor3 = Color3.fromHSV(i,1,1)
                        wait()
                    end
                end
            end)        
        elseif plr.Name == "CaughtInTheAbyss_Alt" then
            ui.PremiumBar.Text.Text = "Abyss"
            ui.PremiumBar.Text.TextColor3 = Color3.fromRGB(0, 0, 0)
            ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(100, 255, 100)
            ui.PremiumBar.Visible = true
            local speed = 5
            spawn(function()
                while true do
                    if not game.CoreGui:FindFirstChild("Horizon") then break end
                    for i = 0,1,0.001*speed do
                        ui.PremiumBar.BackgroundColor3 = Color3.fromHSV(i,1,1)
                        wait()
                    end
                end
            end)
        elseif plr.Name == "Quantavio_alt3" then
            ui.PremiumBar.Text.Text = "Quan"
            ui.PremiumBar.Text.TextColor3 = Color3.fromRGB(0, 0, 0)
            ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(100, 255, 100)
            ui.PremiumBar.Visible = true
            local speed = 5
            spawn(function()
                while true do
                    if not game.CoreGui:FindFirstChild("Horizon") then break end
                    for i = 0,1,0.001*speed do
                        ui.PremiumBar.BackgroundColor3 = Color3.fromHSV(i,1,1)
                        wait()
                    end
                end
            end)
        elseif plr.Name == "xqDerpy" then
            ui.PremiumBar.Text.Text = "Special Derp"
            ui.PremiumBar.Text.TextColor3 = Color3.fromRGB(0, 0, 0)
            ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(255, 215, 0)
            ui.PremiumBar.Visible = true
        elseif plr.Name == "oOFROZEN" then
            ui.PremiumBar.Text.Text = "GigaChad"
            ui.PremiumBar.Text.TextColor3 = Color3.fromRGB(0, 0, 0)
            ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ui.PremiumBar.Visible = true
        elseif plr.Name == "Sinfliction" then
            ui.PremiumBar.Text.Text = ">:3"
            ui.PremiumBar.Text.TextColor3 = Color3.fromRGB(0, 0, 0)
            ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ui.PremiumBar.Visible = true
        elseif plr.Name == "IITheReal_JoeyAltII" then
            ui.PremiumBar.Text.Text = "Special Joey"
            ui.PremiumBar.Text.TextColor3 = Color3.fromRGB(0, 0, 0)
            ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(5, 254, 159)
            ui.PremiumBar.Visible = true
        elseif plr.Name == "IITheReal_JoeyII" then
            ui.PremiumBar.Text.Text = "Special Joey"
            ui.PremiumBar.Text.TextColor3 = Color3.fromRGB(0, 0, 0)
            ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(5, 254, 159)
            ui.PremiumBar.Visible = true
        elseif plr.PrivateStats.FistStrength.Value >= game.Players.LocalPlayer.PrivateStats.BodyToughness.Value * .70 then
            ui.PremiumBar.Text.Text = "Fist Threat"
            ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
            ui.PremiumBar.Visible = true
        elseif plr.PrivateStats.PsychicPower.Value >= game.Players.LocalPlayer.PrivateStats.PsychicPower.Value * 10 then
            ui.PremiumBar.Text.Text = "Psychic Threat"
            ui.PremiumBar.BackgroundColor3 = Color3.fromRGB(50, 50, 255)
            ui.PremiumBar.Visible = true
        end
        ui.UserIcon.Image = game.Players:GetUserThumbnailAsync(plr.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
        ui.StatsBtn.MouseButton1Click:connect(function()
        if SelectedUser ~= plr then
            SelectedUser = plr
            if SelectedUser then
                StatsShowing = true
                Horizon.PLStatsFistTxt.Text = "Fist:\n"..converttoletter(SelectedUser.PrivateStats.FistStrength.Value)
                Horizon.PLStatsBodyTxt.Text = "Body:\n"..converttoletter(SelectedUser.PrivateStats.BodyToughness.Value)
                Horizon.PLStatsPsyTxt.Text = "Psychic:\n"..converttoletter(SelectedUser.PrivateStats.PsychicPower.Value)
                TweenServ:Create(Horizon.PLStatsFrame, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.1}):Play()
                TweenServ:Create(Horizon.PLStatLine, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
                TweenServ:Create(Horizon.PLStatsFist, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.8}):Play()
                TweenServ:Create(Horizon.PLStatsBody, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.8}):Play()
                TweenServ:Create(Horizon.PLStatsPsy, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.8}):Play()
                TweenServ:Create(Horizon.PLSTitle, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                TweenServ:Create(Horizon.PLStatsFistTxt, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                TweenServ:Create(Horizon.PLStatsBodyTxt, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                TweenServ:Create(Horizon.PLStatsPsyTxt, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                TweenServ:Create(Horizon.PLSTitleImage, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
                TweenServ:Create(Horizon.PLStatsFrameShadow, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0.75}):Play()
                Horizon.PLAttackTeleport.Visible = true
                Horizon.PLAttackCBring.Visible = true
                Horizon.PLAttackReap.Visible = true
                Horizon.PLAttackSoulAttack.Visible = true
                TweenServ:Create(Horizon.PLAttackFrame, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.1}):Play()
                TweenServ:Create(Horizon.PLAttackLine, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
                TweenServ:Create(Horizon.PLAttackTitle, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                TweenServ:Create(Horizon.PLAttackTitleImage, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
                TweenServ:Create(Horizon.PLAttackFrameShadow, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0.75}):Play()
                TweenServ:Create(Horizon.PLAttackTeleport, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
                TweenServ:Create(Horizon.PLAttackCBring, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
                TweenServ:Create(Horizon.PLAttackReap, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
                TweenServ:Create(Horizon.PLAttackSoulAttack, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
                TweenServ:Create(Horizon.PLAttackTeleportText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                TweenServ:Create(Horizon.PLAttackCBringText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                TweenServ:Create(Horizon.PLAttackReapText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                TweenServ:Create(Horizon.PLAttackSoulAttackText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
                Horizon.PLAttackCBring.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
                Horizon.PLAttackReap.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
                Horizon.PLAttackSoulAttack.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
                CBringing = false
                LoopReap = false
                LoopSoulAttack = false
            end
        elseif SelectedUser == plr then
            StatsShowing = false
            TweenServ:Create(Horizon.PLStatsFrame, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLStatLine, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLStatsFist, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLStatsBody, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLStatsPsy, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLSTitle, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLStatsFistTxt, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLStatsBodyTxt, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLStatsPsyTxt, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLSTitleImage, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLStatsFrameShadow, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
            Horizon.PLAttackTeleport.Visible = false
            Horizon.PLAttackCBring.Visible = false
            Horizon.PLAttackReap.Visible = false
            Horizon.PLAttackSoulAttack.Visible = false
            TweenServ:Create(Horizon.PLAttackFrame, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLAttackLine, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLAttackTitle, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLAttackTitleImage, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLAttackFrameShadow, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLAttackTeleport, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLAttackCBring, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLAttackReap, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLAttackSoulAttack, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLAttackTeleportText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLAttackCBringText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLAttackReapText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
            TweenServ:Create(Horizon.PLAttackSoulAttackText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
            SelectedUser = nil
        end
       end)
	end
end)
end)

spawn(function()
    game.Players.PlayerRemoving:Connect(function(plr)
        repeat task.wait() until HasFullyExecuted == true
        if pListCreated then
            local ui = Horizon.Apps.PlayerList:FindFirstChild(plr.Name)
            if ui then ui:Destroy() end
            Horizon.Apps.PlayerList.UIListLayout.Padding = UDim.new(0, 5)
            Horizon.Apps.PlayerList.CanvasSize = UDim2.new(0, 0, 0, Horizon.Apps.PlayerList.UIListLayout.AbsoluteContentSize.Y + 2.5)
            Horizon.Apps.PlayerList.UIListLayout.Padding = UDim.new(0, 5)
        end
    end)
end)

local ScriptButtonScale = 0

function newScript(name, color, isToggle, callback)
	local ui = Horizon.ScriptButton:Clone()
	Horizon.Scripts.ActionFrame.ScrollingFrame.UIListLayout.Padding = UDim.new(0, 5)
	if isToggle then
		ui.Execute.TextLabel.Text = "Toggle"
	end
	ui.Parent = Horizon.Scripts.ActionFrame.ScrollingFrame
	ui.Title.Text = name
	if ScriptButtonScale == 0 then ScriptButtonScale = ui.AbsoluteSize.Y end
	ui.Size = UDim2.new(ui.Size.X.Scale, ui.Size.X.Offset, 0, ScriptButtonScale)
	ui.BackgroundColor3 = color
	Horizon.Scripts.ActionFrame.ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, Horizon.Scripts.ActionFrame.ScrollingFrame.UIListLayout.AbsoluteContentSize.Y + 2.5)

	local autoExe = callback(nil, false, true)
	if autoExe then
		ui.AutoExe.Frame.Icon.Visible = true
		autoExe = true
		callback(autoExe, true, false)
	else
		ui.AutoExe.Frame.Icon.Visible = false
		autoExe = false
	end
	ui.AutoExe.Frame.Button.MouseButton1Click:Connect(function()
		autoExe = not autoExe
		if autoExe then
			ui.AutoExe.Frame.Icon.Visible = true
			autoExe = true
			callback(autoExe, false, false)
		else
			ui.AutoExe.Frame.Icon.Visible = false
			autoExe = false
			callback(autoExe, false, false)
		end
	end)
	ui.Execute.Button.MouseButton1Click:Connect(function()
		if not isToggle then
			callback(autoExe, true, false, true)
		else
			callback(autoExe, true, false, true)
		end
	end)
end

game.Players.PlayerRemoving:Connect(function(plr)
	if plr == game.Players.LocalPlayer then
		_G.darkMenu = false
		_G.blackHub = false
        _G.Translator = false
		_G.betterEsp = false
		_G.CmdX = false
		_G.DarkD = false
		_G.MultiCheck = false
        _G.Stack = false
        _G.IY = false
	end
end)

-- ESP 

local Plrs = game:GetService("Players")
local plr = game:GetService("Players").LocalPlayer
local Run = game:GetService("RunService")
local MyPlr = Plrs.LocalPlayer
local MyChar = MyPlr.Character
local CoreGui = game:GetService("CoreGui")
local char = plr.Character

CharAddedEvent = { }

Plrs.PlayerAdded:connect(function(plr)
	if CharAddedEvent[plr.Name] == nil then
		CharAddedEvent[plr.Name] = plr.CharacterAdded:connect(function(char)
			if _G.betterEsp then
                pcall(function()
                    repeat task.wait() until plr.PrivateStats and plr.Character and plr.Character.Head
                    RemoveESP(plr)
                    task.wait()
                    CreateESP(plr)
                end)
			end
		end)
	end
end)

Plrs.PlayerRemoving:connect(function(plr)
	if CharAddedEvent[plr.Name] ~= nil then
		CharAddedEvent[plr.Name]:Disconnect()
		CharAddedEvent[plr.Name] = nil
	end
	RemoveESP(plr)
end)

-- and v ~= JustJoined

local Timeout = math.huge

function CreateESP(plr)
    pcall(function()
        if plr ~= nil then
            plrname = plr.Name
            local GetChar = plr.Character
            local GetHead = GetChar.Head
            
            local bb = Instance.new("BillboardGui", CoreGui)
            bb.Adornee = GetHead
            bb.ExtentsOffset = Vector3.new(0, 1, 0)
            bb.AlwaysOnTop = true
            bb.Size = UDim2.new(0, 5, 0, 5)
            bb.StudsOffset = Vector3.new(0, 3, 0)
            bb.Name = "ESP_" .. plr.Name
            
            local frame = Instance.new("Frame", bb)
            frame.ZIndex = 10
            frame.BackgroundTransparency = 1
            frame.Size = UDim2.new(1, 0, 1, 0)
            
            local TxtName = Instance.new("TextLabel", frame)
            TxtName.Name = "Names"
            TxtName.ZIndex = 10
            TxtName.Text = plr.Name
            TxtName.BackgroundTransparency = 1
            TxtName.Position = UDim2.new(0, 0, 0, -45)
            TxtName.Size = UDim2.new(1, 0, 10, 0)
            TxtName.Font = "SourceSansBold"
            TxtName.TextColor3 = Color3.new(0, 0, 0)
            TxtName.TextSize = 14
            TxtName.TextStrokeTransparency = 0.5
            
            local TxtDist = Instance.new("TextLabel", frame)
            TxtDist.Name = "Dist"
            TxtDist.ZIndex = 10
            TxtDist.Text = ""
            TxtDist.BackgroundTransparency = 1
            TxtDist.Position = UDim2.new(0, 0, 0, -35)
            TxtDist.Size = UDim2.new(1, 0, 10, 0)
            TxtDist.Font = "SourceSansBold"
            TxtDist.TextColor3 = Color3.new(0, 0, 0)
            TxtDist.TextSize = 15
            TxtDist.TextStrokeTransparency = 0.5
    
            local TxtHealth = Instance.new("TextLabel", frame)
            TxtHealth.Name = "Health"
            TxtHealth.ZIndex = 10
            TxtHealth.Text = ""
            TxtHealth.BackgroundTransparency = 1
            TxtHealth.Position = UDim2.new(0, 0, 0, -25)
            TxtHealth.Size = UDim2.new(1, 0, 10, 0)
            TxtHealth.Font = "SourceSansBold"
            TxtHealth.TextColor3 = Color3.new(0, 0, 0)
            TxtHealth.TextSize = 15
            TxtHealth.TextStrokeTransparency = 0.5
    
            local TxtFist = Instance.new("TextLabel", frame)
            TxtFist.Name = "Fist"
            TxtFist.ZIndex = 10
            TxtFist.Text = ""
            TxtFist.BackgroundTransparency = 1
            TxtFist.Position = UDim2.new(0, 0, 0, -15)
            TxtFist.Size = UDim2.new(1, 0, 10, 0)
            TxtFist.Font = "SourceSansBold"
            TxtFist.TextColor3 = Color3.new(0, 0, 0)
            TxtFist.TextSize = 15
            TxtFist.TextStrokeTransparency = 0.5
    
            local TxtPsychic = Instance.new("TextLabel", frame)
            TxtPsychic.Name = "Psychic"
            TxtPsychic.ZIndex = 10
            TxtPsychic.Text = ""
            TxtPsychic.BackgroundTransparency = 1
            TxtPsychic.Position = UDim2.new(0, 0, 0, -5)
            TxtPsychic.Size = UDim2.new(1, 0, 10, 0)
            TxtPsychic.Font = "SourceSansBold"
            TxtPsychic.TextColor3 = Color3.new(0, 0, 0)
            TxtPsychic.TextSize = 15
            TxtPsychic.TextStrokeTransparency = 0.5
        end
    end)
end

function UpdateESP(plr)
	local Find = CoreGui:FindFirstChild("ESP_" .. plr.Name)
	if Find then
		local plrStatus = game.Players[plr.Name].leaderstats.Status
		if plrStatus.Value == "Criminal" then
			Find.Frame.Names.TextColor3 = Color3.new(1, 0.1, 1)
		elseif plrStatus.Value == "Lawbreaker" then
			Find.Frame.Names.TextColor3 = Color3.new(1, 0.1, 0.1)
		elseif plrStatus.Value == "Guardian" then
			Find.Frame.Names.TextColor3 = Color3.new(0.1, 0.8, 1)
		elseif plrStatus.Value == "Protector" then
			Find.Frame.Names.TextColor3 = Color3.new(0.1, 0.1, 1)
		elseif plrStatus.Value == "Supervillain" then
			Find.Frame.Names.TextColor3 = Color3.new(0.3, 0.1, 0.1)
		elseif plrStatus.Value == "Superhero" then
			Find.Frame.Names.TextColor3 = Color3.new(0.8, 0.8, 0)
		else
			Find.Frame.Names.TextColor3 = Color3.new(1, 1, 1)
		end
		Find.Frame.Dist.TextColor3 = Color3.new(1, 1, 1)
		Find.Frame.Health.TextColor3 = Color3.new(1, 1, 1)
		Find.Frame.Fist.TextColor3 = Color3.new(1, 1, 1)
		Find.Frame.Psychic.TextColor3 = Color3.new(1, 1, 1)
		local GetChar = plr.Character
		if MyChar and GetChar then
			local Find2 = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart", Timeout)
			local Find3 = GetChar:FindFirstChild("HumanoidRootPart")
			local Find4 = GetChar:FindFirstChild("Humanoid")
			if Find3 then
				if not Find2 then return end
				local pos = Find3.Position
				local Dist = (Find2.Position - pos).magnitude
                Find.Frame.Names.Visible = true
                Find.Frame.Dist.Visible = true
                Find.Frame.Health.Visible = true
                Find.Frame.Fist.Visible = true
                Find.Frame.Psychic.Visible = true
				Find.Frame.Dist.Text = "Distance: " .. string.format("%.0f", Dist)
				--Find.Frame.Pos.Text = "(X: " .. string.format("%.0f", pos.X) .. ", Y: " .. string.format("%.0f", pos.Y) .. ", Z: " .. string.format("%.0f", pos.Z) .. ")"
				if Find4 then
					Find.Frame.Dist.Text = "Distance: " .. string.format("%.0f", Dist)
					Find.Frame.Health.Text = "Health: " ..converttoletter(string.format("%.0f", Find4.Health))
					Find.Frame.Fist.Text = "Fist: " ..converttoletter(string.format("%.0f", game.Players[plr.Name].PrivateStats.FistStrength.Value))
					Find.Frame.Psychic.Text = "Psychic: " ..converttoletter(string.format("%.0f", game.Players[plr.Name].PrivateStats.PsychicPower.Value))
				else
					Find.Frame.Health.Text = ""
					Find.Frame.Fist.Text = ""
					Find.Frame.Psychic.Text = ""
				end
			end
		end
	end
end

function RemoveESP(plr)
	local ESP = game.CoreGui:FindFirstChild("ESP_" .. plr.Name)
	if ESP then
		ESP:Destroy()
	end
end

Run:BindToRenderStep("UpdateESP", Enum.RenderPriority.Character.Value, function()
	for _, v in next, Plrs:GetPlayers() do
		UpdateESP(v)
	end
end)

spawn(function()
newScript("Better ESP", Color3.fromRGB(20, 20, 20), true, function(value, exe, isCheck, isForce)
	if not exe and not isCheck then BetterEspAutoExe = value end
	if isCheck and not exe then return BetterEspAutoExe end
	if exe and not isCheck then
        if not _G.betterEsp or isForce then
            spawn(function()
                _G.betterEsp = not _G.betterEsp
                if _G.betterEsp then
                    pcall(function()
                        for _, v in pairs(Plrs:GetPlayers()) do
                            spawn(function()
                                if v ~= MyPlr then
                                    if v.Character then
                                        if CharAddedEvent[v.Name] == nil then
                                            CharAddedEvent[v.Name] = v.CharacterAdded:connect(function(Char)
                                                pcall(function()
                                                    repeat task.wait() until Char.Head and Char.Humanoid and Char.HumanoidRootPart
                                                    if _G.betterEsp then
                                                        RemoveESP(v)
                                                        CreateESP(v)
                                                    end
                                                end)
                                            end)
                                        end
                                        pcall(function()
                                            repeat task.wait() until v.Character.Head
                                        end)
                                        RemoveESP(v)
                                        CreateESP(v)
                                    else
                                        repeat task.wait() until v.Character
                                    end
                                end
                            end)
                        end
                    end)
                else
                    for _, v in next, Plrs:GetPlayers() do
                        RemoveESP(v)
                    end
                end
            end)
        end
	end
end)

spawn(function()
	while task.wait() do
        if not game.CoreGui:FindFirstChild("Horizon") then break end
        pcall(function()
            for i,v in pairs(game.CoreGui:GetChildren()) do
                if string.find(v.Name, "ESP_") then
                    repeat task.wait() until game.Workspace:FindFirstChild(string.gsub(v.Name, "ESP_", "")).Head
                    v.Adornee = game.Workspace:FindFirstChild(string.gsub(v.Name, "ESP_", "")).Head
                end
            end
        end)
	end
end)

spawn(function()
	while task.wait(1) and StopCamera ~= true do
        if not game.CoreGui:FindFirstChild("Horizon") then break end
        if _G.betterEsp then
            pcall(function()
                for _, v in pairs(Plrs:GetPlayers()) do
                    RemoveESP(v)
                    if v ~= MyPlr then
                        pcall(function()
                            if v.Character and v.Character.Head and StopCamera ~= true then
                                RemoveESP(v)
                                CreateESP(v)
                            else
                                repeat task.wait() until v.Character
                                repeat task.wait() until v.Character.Head
                            end
                        end)
                    end
                end
            end)
        else
            for _, v in next, Plrs:GetPlayers() do
                RemoveESP(v)
            end
        end
    end
end)

newScript("(Old) Abyss Hub V5", Color3.fromRGB(20, 20, 20), false, function(value, exe, isCheck, isForece)
	if not exe and not isCheck then CmdXAutoExe = value end
	if isCheck and not exe then return CmdXAutoExe end
	if exe and not isCheck then
		if not _G.CmdX or isForce then
			_G.CmdX = true
            spawn(function()
                loadstring(game:HttpGet("https://raw.githubusercontent.com/CaughtInTheAbyss/AbyssHubV4/main/AbyssHub", true))()
            end)
			_G.CmdX = false
		end
	end
end)

newScript("Infinite Yield", Color3.fromRGB(20, 20, 20), false, function(value, exe, isCheck, isForece)
	if not exe and not isCheck then IYAutoExec = value end
	if isCheck and not exe then return IYAutoExec end
	if exe and not isCheck then
		if not _G.IY or isForce then
			_G.IY = true
            spawn(function()
                loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source", true))()
            end)
			_G.IY = false
		end
	end
end)

