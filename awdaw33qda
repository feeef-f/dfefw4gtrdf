function UpdateFriends()
    local friendsId = game.Players.LocalPlayer.UserId
    local PlayersFriends = {}
    local success, page = pcall(function() return game:GetService("Players"):GetFriendsAsync(friendsId) end)
    if success then
        repeat
            local info = page:GetCurrentPage()
            for i, friendInfo in pairs(info) do
                table.insert(PlayersFriends, friendInfo)
            end
            if not page.IsFinished then 
                page:AdvanceToNextPageAsync()
            end
        until page.IsFinished
    end
    local FriendsInTotal = 0
    local OnlineFriends = 0 
    local FriendsInGame = 0 
    for i,v in pairs(PlayersFriends) do
        FriendsInTotal  = FriendsInTotal + 1
        if v.IsOnline then
            OnlineFriends = OnlineFriends + 1
        end
        if game.Players:FindFirstChild(v.Username) then
            FriendsInGame = FriendsInGame + 1
        end
    end
    Horizon.FriendsAll.Text = tostring(FriendsInTotal).." users"
    Horizon.FriendsOffline.Text = tostring(FriendsInTotal - OnlineFriends).." users"
    Horizon.FriendsOnline.Text = tostring(OnlineFriends).." users"
    Horizon.FriendsInGame.Text = tostring(FriendsInGame).." users"
end

UpdateFriends()

spawn(function()
    while task.wait(30) do
        if not game.CoreGui:FindFirstChild("Horizon") then break end
        UpdateFriends()
    end
end)

spawn(function()
	while task.wait(0.3) do
        if not game.CoreGui:FindFirstChild("Horizon") then break end
		Horizon.HomeStat1.Text = "Fist: "..converttoletter(game:GetService("Players").LocalPlayer.PrivateStats.FistStrength.Value)
		Horizon.HomeStat2.Text = "Body: "..converttoletter(game:GetService("Players").LocalPlayer.PrivateStats.BodyToughness.Value)
		Horizon.HomeStat3.Text = "Psychic: "..converttoletter(game:GetService("Players").LocalPlayer.PrivateStats.PsychicPower.Value)
	end
end)

spawn(function()
	while task.wait(0.1) do
        if not game.CoreGui:FindFirstChild("Horizon") then break end
		local TimeInUnix = os.time()
		local stringToFormat = "%H:%M"
		local result = os.date(stringToFormat, TimeInUnix)
		Horizon.TimeLabel.Text = result
	end
end)

function OpenHome()
    if AppsOpen then
        CloseApps()
        GlowOn = true
    elseif ScriptsOpen then
        CloseScripts()
        GlowOn = true
    elseif SettingsOpen then
        CloseSettings()
        GlowOn = true
    end
    HomeOpen = true
    if GlowOn == false then
        GlowOn = true
        Horizon.BarMover.Position = UDim2.new(0.1, 0, 0.5, 0)
        TweenServ:Create(Horizon.BarMover, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {BackgroundTransparency = 0}):Play()
    elseif GlowOn then
        TweenServ:Create(Horizon.BarMover, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {Position = UDim2.new(0.1, 0, 0.5, 0)}):Play()
        TweenServ:Create(Horizon.BarMover, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {BackgroundTransparency = 0}):Play()
    end
    if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
        TweenServ:Create(Horizon.Button1Icon, TweenInfo.new(0.4, Enum.EasingStyle.Exponential), {ImageColor3 = Color3.fromRGB(10, 10, 10)}):Play()
    end
    TweenServ:Create(Horizon.Backdrop, TweenInfo.new(0.4, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    game:GetService("CoreGui").PlayerList.Enabled = false
    game:GetService("CoreGui").RobloxGui.Backpack.Visible = false
    game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.MainQuestFrame.Visible = false
    game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.DailyQuestFrame.Visible = false
    game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.WeeklyQuestFrame.Visible = false
    game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.SpecialQuestFrame.Visible = false
    Horizon.BarToggle.Visible = false
    TweenServ:Create(MusicTable.Music, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {Position = UDim2.new(0.88, 0, 0.85, 0)}):Play()
    TweenServ:Create(Horizon.HomeCard, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {Position = UDim2.new(0.13, 0, 0.165, 0)}):Play()
    TweenServ:Create(Horizon.Friends, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {Position = UDim2.new(0.755, 0, 0.04, 0)}):Play()
    TweenServ:Create(Horizon.Data, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {Position = UDim2.new(0.005, 0, 0.3, 0)}):Play()
    TweenServ:Create(Horizon.CloseButton, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {Position = UDim2.new(0.48, 0, 0.01, 0)}):Play()
end

function CloseHome()
    HomeOpen = false
    game:GetService("CoreGui").PlayerList.Enabled = true
    if GlowOn then
        GlowOn = false
        TweenServ:Create(Horizon.BarMover, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {BackgroundTransparency = 1}):Play()
    end
    if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
        TweenServ:Create(Horizon.Button1Icon, TweenInfo.new(0.4, Enum.EasingStyle.Exponential), {ImageColor3 = Color3.fromRGB(255, 255, 255)}):Play()
    end
    TweenServ:Create(MusicTable.Music, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {Position = UDim2.new(1.2, 0, 0.85, 0)}):Play()
    TweenServ:Create(Horizon.HomeCard, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {Position = UDim2.new(-0.2, 0, 0.165, 0)}):Play()
    TweenServ:Create(Horizon.Friends, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {Position = UDim2.new(1.2, 0, 0.04, 0)}):Play()
    TweenServ:Create(Horizon.Data, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {Position = UDim2.new(-0.4, 0, 0.3, 0)}):Play()
    TweenServ:Create(Horizon.CloseButton, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {Position = UDim2.new(0.48, 0, -0.2, 0)}):Play()
    TweenServ:Create(Horizon.Backdrop, TweenInfo.new(0.4, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    Horizon.BarToggle.Visible = true
    game:GetService("CoreGui").RobloxGui.Backpack.Visible = true
    game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.MainQuestFrame.Visible = true
end

function OpenApps()
    if HomeOpen then
        CloseHome()
        GlowOn = true
    elseif ScriptsOpen then
        CloseScripts()
        GlowOn = true
    elseif SettingsOpen then
        CloseSettings()
        GlowOn = true
    end
    AppsOpen = true
    if GlowOn == false then
        GlowOn = true
        Horizon.BarMover.Position = UDim2.new(0.3, 0, 0.5, 0)
        TweenServ:Create(Horizon.BarMover, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {BackgroundTransparency = 0}):Play()
    elseif GlowOn then
        TweenServ:Create(Horizon.BarMover, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {Position = UDim2.new(0.3, 0, 0.5, 0)}):Play()
        TweenServ:Create(Horizon.BarMover, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {BackgroundTransparency = 0}):Play()
    end
    if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
        TweenServ:Create(Horizon.Button2Icon, TweenInfo.new(0.4, Enum.EasingStyle.Exponential), {ImageColor3 = Color3.fromRGB(10, 10, 10)}):Play()
    end
    game:GetService("CoreGui").Horizon.Apps.PlayerList.Visible = true
    Horizon.Icon_8.Visible = true
    Horizon.BarToggle.Visible = false
    TweenServ:Create(Horizon.Icon_8, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
    TweenServ:Create(Horizon.Apps, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.1}):Play()
    TweenServ:Create(Horizon.AppsLine, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
    TweenServ:Create(Horizon.AppsTitle, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
    TweenServ:Create(Horizon.AppsTitleImage, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
    TweenServ:Create(Horizon.Shadow, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0.75}):Play()
    Horizon.peopleSearchBar.Visible = true
    if StatsShowing == true then
        TweenServ:Create(Horizon.PLStatsFrame, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.1}):Play()
        TweenServ:Create(Horizon.PLStatLine, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
        TweenServ:Create(Horizon.PLStatsFist, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.8}):Play()
        TweenServ:Create(Horizon.PLStatsBody, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.8}):Play()
        TweenServ:Create(Horizon.PLStatsPsy, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.8}):Play()
        TweenServ:Create(Horizon.PLSTitle, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
        TweenServ:Create(Horizon.PLStatsFistTxt, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
        TweenServ:Create(Horizon.PLStatsBodyTxt, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
        TweenServ:Create(Horizon.PLStatsPsyTxt, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
        TweenServ:Create(Horizon.PLSTitleImage, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
        TweenServ:Create(Horizon.PLStatsFrameShadow, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0.75}):Play()
        TweenServ:Create(Horizon.PLAttackFrame, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0.1}):Play()
        TweenServ:Create(Horizon.PLAttackLine, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
        TweenServ:Create(Horizon.PLAttackTitle, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
        TweenServ:Create(Horizon.PLAttackTitleImage, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0}):Play()
        TweenServ:Create(Horizon.PLAttackFrameShadow, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 0.75}):Play()
        Horizon.PLAttackTeleport.Visible = true
        Horizon.PLAttackCBring.Visible = true
        Horizon.PLAttackReap.Visible = true
        Horizon.PLAttackSoulAttack.Visible = true
        TweenServ:Create(Horizon.PLAttackTeleport, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
        TweenServ:Create(Horizon.PLAttackCBring, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
        TweenServ:Create(Horizon.PLAttackReap, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
        TweenServ:Create(Horizon.PLAttackSoulAttack, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 0}):Play()
        TweenServ:Create(Horizon.PLAttackTeleportText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
        TweenServ:Create(Horizon.PLAttackCBringText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
        TweenServ:Create(Horizon.PLAttackReapText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
        TweenServ:Create(Horizon.PLAttackSoulAttackText, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 0}):Play()
    end
end

function CloseApps()
    AppsOpen = false
    Horizon.BarToggle.Visible = true
    if GlowOn then
        GlowOn = false
        TweenServ:Create(Horizon.BarMover, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {BackgroundTransparency = 1}):Play()
    end
    if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
        TweenServ:Create(Horizon.Button2Icon, TweenInfo.new(0.4, Enum.EasingStyle.Exponential), {ImageColor3 = Color3.fromRGB(255, 255, 255)}):Play()
    end
    game:GetService("CoreGui").Horizon.Apps.PlayerList.Visible = false
    TweenServ:Create(Horizon.Icon_8, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
    Horizon.Icon_8.Visible = false
    TweenServ:Create(Horizon.Apps, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.AppsLine, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.AppsTitle, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {TextTransparency = 1}):Play()
    TweenServ:Create(Horizon.AppsTitleImage, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
    TweenServ:Create(Horizon.Shadow, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {ImageTransparency = 1}):Play()
    Horizon.peopleSearchBar.Visible = false
    TweenServ:Create(Horizon.PLStatsFrame, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
    TweenServ:Create(Horizon.PLStatLine, TweenInfo.new(0.5, Enum.EasingStyle.Exponential), {BackgroundTransparency = 1}):Play()
