Horizon.Interact2.MouseButton1Click:connect(function()
    if RespawnActive == false then
        if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
            Horizon.SettingsButton2.BackgroundColor3 = Color3.fromRGB(69, 255, 113)
            Horizon.Interact2.ImageColor3 = Color3.fromRGB(10, 10, 10)
        else
            Horizon.Interact2.ImageColor3 = Color3.fromRGB(255, 255, 255)
            Horizon.SettingsButton2.BackgroundColor3 = Color3.fromRGB(144, 41, 246)
        end
        RespawnActive = true
    elseif RespawnActive == true then
        Horizon.Interact2.ImageColor3 = Color3.fromRGB(255, 255, 255)
        Horizon.SettingsButton2.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        RespawnActive = false
    end
end)

Horizon.Interact3.MouseButton1Click:connect(function()
    if SuperAura == false then
        if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
            Horizon.SettingsButton3.BackgroundColor3 = Color3.fromRGB(69, 255, 113)
            Horizon.Interact3.ImageColor3 = Color3.fromRGB(10, 10, 10)
        else
            Horizon.Interact3.ImageColor3 = Color3.fromRGB(255, 255, 255)
            Horizon.SettingsButton3.BackgroundColor3 = Color3.fromRGB(144, 41, 246)
        end
        SuperAura = true
        RespawnAP()
        Horizon.SettingsButton4.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        Horizon.Interact4.ImageColor3 = Color3.fromRGB(255, 255, 255)
        NoAura = false
    elseif SuperAura == true then
        Horizon.Interact3.ImageColor3 = Color3.fromRGB(255, 255, 255)
        Horizon.SettingsButton3.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        SuperAura = false
    end
end)

Horizon.Interact4.MouseButton1Click:connect(function()
    if NoAura == false then
        if game.Players.LocalPlayer.Name == "CaughtInTheAbyss" or game.Players.LocalPlayer.Name == "oOFROZEN" or game.Players.LocalPlayer.Name == "Quantavio_alt3" or game.Players.LocalPlayer.Name == "CaughtInTheAbyss_Alt" then
            Horizon.SettingsButton4.BackgroundColor3 = Color3.fromRGB(69, 255, 113)
            Horizon.Interact4.ImageColor3 = Color3.fromRGB(10, 10, 10)
        else
            Horizon.Interact4.ImageColor3 = Color3.fromRGB(255, 255, 255)
            Horizon.SettingsButton4.BackgroundColor3 = Color3.fromRGB(144, 41, 246)
        end
        NoAura = true
        Horizon.SettingsButton3.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        Horizon.Interact3.ImageColor3 = Color3.fromRGB(255, 255, 255)
        SuperAura = false
    elseif NoAura == true then
        Horizon.Interact4.ImageColor3 = Color3.fromRGB(255, 255, 255)
        Horizon.SettingsButton4.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        RespawnAP()
        NoAura = false
    end
end)

