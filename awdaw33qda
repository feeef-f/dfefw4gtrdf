Horizon.CloseButtonImage.MouseButton1Click:Connect(function()
    getgenv().HorizonExecuted = false
    spawn(function()
        for _,v in pairs(game.CoreGui:GetChildren()) do
            if v.Name == "Horizon" then
                v:Destroy()
            elseif v.Name == "Load" then
                v:Destroy()
            elseif v.Name == "Notif" then
                v:Destroy()
            end
        end
        game:GetService("CoreGui").PlayerList.Enabled = true
        game:GetService("CoreGui").RobloxGui.Backpack.Visible = true
        game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.MainQuestFrame.Visible = true
        TweenServ:Create(game:GetService("Workspace").Camera, TweenInfo.new(0.2, Enum.EasingStyle.Exponential), {FieldOfView = 70}):Play()
    end)
end)

