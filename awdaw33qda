if game.PlaceId ~= 15786769057 then return end

if not game:IsLoaded() then
    game.Loaded:wait()
end

if getgenv().HorizonExecuted == true then return end
getgenv().HorizonExecuted = true

    --Please don't spam the webhook. I don't even log IP's :/


    local plr = game.Players.LocalPlayer
    local OSTime = os.time()
    local Time = os.date('!*t', OSTime)
    
    local function getexploit()
        local exploit =
            (syn and not is_sirhurt_closure and not pebc_execute and "Synapse X") or 
            (isexecutorclosure and "Script-Ware V2") or
            (secure_load and "Sentinel") or
            (is_sirhurt_closure and "SirHurt V4") or
            (pebc_execute and "ProtoSmasher") or
            (KRNL_LOADED and "Krnl") or
            (WrapGlobal and "WeAreDevs") or
            (isvm and "Proxo") or
            (shadow_env and "Shadow") or
            (jit and "EasyExploits") or
            (getreg()['CalamariLuaEnv'] and "Calamari") or
            (unit and "Unit") or
            ("Undetectable")
        return exploit
    end
    
    --Please don't spam the webhook. I don't even log IP's :/
    local Content = 'Someone executed Horizon Revanced!'
    local Embed = {
                ["title"] = "__**New execution.**__",
                ["description"] = "Name: "..plr.Name.."\nDisplay Name: "..plr.DisplayName.."\nExploit: "..getexploit().."\nGame: https://www.roblox.com/games/"..game.PlaceId,
                ["type"] = "rich",
                ["color"] = tonumber(0xffff00),
                        ["thumbnail"] = {
                    ["url"] = "https://www.roblox.com/asset-thumbnail/image?assetId="..game.PlaceId.."&width=768&height=432"
                },
                ["image"] = {
                    ["url"] = "http://www.roblox.com/Thumbs/Avatar.ashx?x=250&y=250&Format=Png&username="..plr.Name
                },
                ["fields"] = {
                    {
                        ["name"] = "__Account Age:__",
                        ["value"] = plr.AccountAge.." days old",
                        ["inline"] = true
                    },
                    {
                        ["name"] = "__User ID:__",
                        ["value"] = plr.UserId,
                        ["inline"] = true
                    }
                },
                ["footer"] = {
                    ["text"] = "Horizon Revanced",
                    ["icon_url"] = "https://i.imgur.com/Vz2XUGj.jpg"
                },
                ["timestamp"] = string.format('%d-%d-%dT%02d:%02d:%02dZ', Time.year, Time.month, Time.day, Time.hour, Time.min, Time.sec),
    };
    (syn and syn.request or http_request or http.request) {
        Url = 'https://discord.com/api/webhooks/1163505464676454500/qFTunhwqhQooLywAUlkkRvqU983UJ71LMvuG8KOJsLrWXE8TZk1SWdX990fxW4snwuQV';
        Method = 'POST';
        Headers = {
            ['Content-Type'] = 'application/json';
        };
        Body = game:GetService'HttpService':JSONEncode({content = Content; embeds = {Embed}; });
    };
    
    
    --Please don't spam the webhook. I don't even log IP's :/

spawn(function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/CaughtInTheAbyss/Secret/main/audio', true))()
end)

for _,v in pairs(game.CoreGui:GetChildren()) do
    if v.Name == "Horizon" or v.Name == "Load" then
        v:Destroy()
    end
end
for _,v in pairs(game.ReplicatedStorage:GetChildren()) do
    if v.Name == "Horizon_Assets" then
        v:Destroy()
    end
end

local Release = "rbxUWP"
local HomeOpen = false
local AppsOpen = false
