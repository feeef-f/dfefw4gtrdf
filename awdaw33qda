for i,v in pairs(game.CoreGui.Horizon:GetDescendants()) do
	if v:IsA("ImageLabel") or v:IsA("ImageButton") then
        table.insert(preload, v.Image)
    end
end

game:GetService("ContentProvider"):PreloadAsync(preload)

